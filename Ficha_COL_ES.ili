INTERLIS 2.3;

MODEL Ficha_COL_ES_V2_2 (es)
AT "mailto:vmbp@localhost"
VERSION "2017-06-02 V2.1.1"  =
  IMPORTS Catastro_COL_ES_V2_2_0,Avaluos_COL_ES_V2_1;

  DOMAIN

    COL_ArmazonTipo = (
      Madera,
      Prefabricado,
