    );

    COL_CalidadMobiliarioTipo = (
      Pobre,
      Sencillo,
      Regular,
      Bueno,
      Lujoso
    );

    COL_CategoriaQuienAtendioTipo = (
      Propietario,
      Arrendatario,
      Cuidandero,
      Visitante,
      Visitante_Ocasional,
      Otro
    );

    COL_CategoriaSueloPOTTipo = (
      Suburbano,
      Proteccion
    );

    COL_CerchasTipo = (
      Madera,
      Metalica_Liviana,
      Metalica_Mediana,
      Metalica_Pesada,
      Altura
    );

    COL_ClaseSueloPOTTipo = (
