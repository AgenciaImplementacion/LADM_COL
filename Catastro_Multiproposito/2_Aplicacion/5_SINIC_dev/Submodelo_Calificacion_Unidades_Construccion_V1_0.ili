       */
      !!@ ili2db.dispName = "Puntos"
      !!@ ili2db.dispName="Puntos"
      Puntos : MANDATORY 0 .. 16;
    END CUC_ObjetoConstruccion;

    /** Proceso mediante el cual se le asigna un puntaje a las edificaciones de acuerdo a sus características constructivas
     */
    !!@ ili2db.dispName = "Calificación Convencional"
    !!@ ili2db.dispName="Calificación Convencional"
    CLASS CUC_CalificacionConvencional
    EXTENDS CUC_CalificacionUnidadConstruccion =
      /** Indica si la calificación de construcción corresponde a residencial, comercial o industrial.
       */
      !!@ ili2db.dispName = "Tipo a calificar"
      !!@ ili2db.dispName="Tipo a calificar"
      Tipo_Calificar : MANDATORY Submodelo_Calificacion_Unidades_Construccion_V1_0.CUC_CalificarTipo;
      /** Puntaje total de la calificación de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total calificación"
      !!@ ili2db.dispName="Total calificación"
      Total_Calificacion : MANDATORY LADM_COL_V3_0.LADM_Nucleo.Integer;
    END CUC_CalificacionConvencional;

    /** Son las características de diseño y constructivas especiales de las construcciones y/o edificaciones.
     */
    !!@ ili2db.dispName = "Tipología de la construcción"
    !!@ ili2db.dispName="Tipología de la construcción"
    CLASS CUC_TipologiaConstruccion
    EXTENDS CUC_CalificacionUnidadConstruccion =
      /** Son las características de diseño y constructivas especiales de las construcciones y/o edificaciones.
       */
      !!@ ili2db.dispName = "Tipo de tipología"
      !!@ ili2db.dispName="Tipo de tipología"
      Tipo_Tipologia : MANDATORY Submodelo_Calificacion_Unidades_Construccion_V1_0.CUC_TipologiaTipo;
    END CUC_TipologiaConstruccion;

    /** Relación entre la clase Unidad_Construccion del modelo de Avalúos y Calificacion_Unidad_Construccion de modelo de Avalúos
     */
    ASSOCIATION cuc_calificacion_unidadconstruccion =
      sinic_unidad_construccion (EXTERNAL) -- {1..*} Modelo_Aplicacion_LADMCOL_SINIC_V1_0.Sistema_Nacional_Informacion_Catastral.SINIC_UnidadConstruccion;
      cuc_calificacion_unidad_construccion -- {0..1} CUC_CalificacionUnidadConstruccion;
    END cuc_calificacion_unidadconstruccion;

    ASSOCIATION cuc_objetoconstruccion_grupocalificacion =
      cuc_grupo_calificacion -- {1} CUC_GrupoCalificacion;
      cuc_objeto_construccion -- {0..*} CUC_ObjetoConstruccion;
    END cuc_objetoconstruccion_grupocalificacion;

    /** Relaciona el tipo de construcción consideradas como anexo para el catastro.
     */
    !!@ ili2db.dispName = "Calificación No Convencional"
    !!@ ili2db.dispName="Calificación No Convencional"
    CLASS CUC_CalificacionNoConvencional
    EXTENDS CUC_CalificacionUnidadConstruccion =
      /** Relaciona el tipo de construcción consideradas como anexo para el catastro.
       */
      !!@ ili2db.dispName = "Tipo de anexo"
      !!@ ili2db.dispName="Tipo de anexo"
      Tipo_Anexo : MANDATORY Submodelo_Calificacion_Unidades_Construccion_V1_0.CUC_AnexoTipo;
    END CUC_CalificacionNoConvencional;

    ASSOCIATION cuc_grupo_calificacionconvencional =
      cuc_calificacion_convencional -- {1} CUC_CalificacionConvencional;
      cuc_grupo_calificacion -- {1..*} CUC_GrupoCalificacion;
    END cuc_grupo_calificacionconvencional;

  END Calificacion_Unidad_Construccion;

END Submodelo_Calificacion_Unidades_Construccion_V1_0.
