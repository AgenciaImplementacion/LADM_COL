INTERLIS 2.3;

MODEL Submodelo_Datos_Levantamiento_Catastral_V1_0 (es)
AT "mailto:PC-1@localhost"
VERSION "2020-12-03"  =
  IMPORTS Modelo_Aplicacion_LADMCOL_SINIC_V1_0;

  DOMAIN

    DLC_ProcedimientoCatastralRegistralTipo = (
      /** El gestor catastral competente, de oficio o a solicitud de parte del titular del derecho de dominio, o de las entidades públicas que con ocasión de sus funciones legales administran inmuebles propios o ajenos, podrán efectuar la actualización mediante la descripción técnica de linderos de bienes inmuebles, cuando sean verificables mediante métodos directos y/o indirectos sin variación, o cuando la variación o diferencia se encuentre dentro de los márgenes de tolerancia establecidos por la máxima autoridad catastral. La descripción de los linderos llevará a la certeza del área. Articulo 2.2.2.2.17. Decreto 148 de 2020.
       */
      !!@ ili2db.dispName = "Actualización linderos con efectos registrales"
      !!@ ili2db.dispName="Actualización linderos con efectos registrales"
      Actualizacion_Linderos,
      /** La rectificación de área en el sistema catastral y/o registral procederá ante el gestor catastral, de oficio o a solicitud de parte del titular  del derecho de dominio, o de las entidades públicas que con ocasión de sus funciones legales administran inmuebles propios o ajenos, cuando los linderos estén debida y técnicamente descritos, sean verificables mediante métodos directos y/o indirectos sin variación, pero que a lo largo de la tradición del bien inmueble el área de éste no haya sido determinada adecuadamente, o cuando la variación o diferencia se encuentre dentro de los márgenes de tolerancia establecidos por la máxima autoridad catastral. Artículo 2.2.2.2.17. Decreto 148 de 2020.
       */
      !!@ ili2db.dispName = "Rectificación de área por imprecisa determinación con efectos registrales"
      !!@ ili2db.dispName="Rectificación de área por imprecisa determinación con efectos registrales"
      Rectificacion_Area,
      /** La rectificación de linderos en el sistema catastral y registral procederá de oficio o a solicitud de parte, ante el gestor catastral, siempre y cuando se haya suscrito acta de colindancia con pleno acuerdo entre los propietarios que compartan uno o varios linderos, pero se adviertan diferencias de linderos y áreas, entre la verificación mediante métodos directos y/o indirectos y la información del folio de matricula inmobiliaria. La rectificación de todos los linderos puede llevar a la certeza del área del inmueble. Artículo 2.2.2.2.17. Decreto 148 de 2020.
       */
      !!@ ili2db.dispName = "Rectificación de linderos por acuerdo entre las partes con efectos registrales"
      !!@ ili2db.dispName="Rectificación de linderos por acuerdo entre las partes con efectos registrales"
      Rectificacion_Linderos,
      /** n los casos donde los folios de matrícula inmobiliaria no hayan contado con información de área y/o linderos, desde el inicio del ciclo traslaticio del bien
       * inmueble que identifican; la Oficina de Registro de Instrumentos Públicos procederá la inclusión del dato de área y/o linderos en las respectivas matrículas inmobiliarias, que se tomará de la base catastral administrada por el gestor catastral competente.
       */
      !!@ ili2db.dispName = "Inclusión en el campo de cabida y linderos del dato del área y/o linderos en los folios de matrícula inmobiliaria"
      !!@ ili2db.dispName="Inclusión en el campo de cabida y linderos del dato del área y/o linderos en los folios de matrícula inmobiliaria"
      Inclusion_Cabida_Linderos,
      /** No requiere procedimiento catastral con efecto registral.
       */
      !!@ ili2db.dispName = "No requiere procedimiento"
      !!@ ili2db.dispName="No requiere procedimiento"
      No_Requiere
    );

    DLC_RelacionPredioTipo = (
      /** Parte interesada (Propietario, Poseedor, Ocupante)
       */
      !!@ ili2db.dispName = "Parte interesada (Propietario, Poseedor, Ocupante)"
      !!@ ili2db.dispName="Parte interesada (Propietario, Poseedor, Ocupante)"
      Parte_Interesada,
      /** Indica si un vecino de la parte interesada (propietario, poseedor u ocupante) fue el encargado de atender la visita predial.
       */
      !!@ ili2db.dispName = "Vecino"
      !!@ ili2db.dispName="Vecino"
      Vecino,
      /** Indica si un familiar de la parte interesada (propietario, poseedor u ocupante) fue el encargado de atender la visita predial.
       */
      !!@ ili2db.dispName = "Familiar"
      !!@ ili2db.dispName="Familiar"
      Familiar,
      /** Indica si el arrendatario o alguna persona de su núcleo familiar fue el encargado de atender la visita predial.
       */
      !!@ ili2db.dispName = "Arrendatario"
      !!@ ili2db.dispName="Arrendatario"
      Arrendatario,
      /** Indica si el administrador o alguna persona de su núcleo familiar fue el encargado de atender la visita predial.
       */
      !!@ ili2db.dispName = "Administrador"
      !!@ ili2db.dispName="Administrador"
      Administrador
    );

    DLC_ResultadoVisitaTipo = (
      /** Tuvo acceso total al predio
       */
      !!@ ili2db.dispName = "Exitoso"
      !!@ ili2db.dispName="Exitoso"
      Exitoso,
      /** Tuvo acceso parcial al predio.
       */
      !!@ ili2db.dispName = "Incompleto"
      !!@ ili2db.dispName="Incompleto"
      Incompleto,
      /** Sin atención por parte de personas. Predio Desocupado.
       */
      !!@ ili2db.dispName = "No hay nadie"
      !!@ ili2db.dispName="No hay nadie"
      No_hay_nadie,
      /** No permitieron acceso al predio.
       */
      !!@ ili2db.dispName = "No permitieron acceso"
      !!@ ili2db.dispName="No permitieron acceso"
      No_permitieron_acceso,
      /** No hay adulto en el momento de la visita.
       */
      !!@ ili2db.dispName = "Menor de Edad"
      !!@ ili2db.dispName="Menor de Edad"
      Menor_de_Edad,
      /** Por temas de seguridad asociados a orden público no es posible el acceso.
       */
      !!@ ili2db.dispName = "Situación de Orden Público"
      !!@ ili2db.dispName="Situación de Orden Público"
      Situacion_de_Orden_Publico,
      /** Por limitaciones relacionadas con la topografía o accidentes geográficos no hubo acceso al predio.
       */
      !!@ ili2db.dispName = "Zona de difícil acceso"
      !!@ ili2db.dispName="Zona de difícil acceso"
      Zona_de_dificil_acceso,
      /** Otros casos no relacionados anteriormente.
       */
      !!@ ili2db.dispName = "Otro"
      !!@ ili2db.dispName="Otro"
      Otro
    );

  TOPIC Datos_Levantamiento_Catastral =
    DEPENDS ON Modelo_Aplicacion_LADMCOL_SINIC_V1_0.Sistema_Nacional_Informacion_Catastral;

    !!@ ili2db.dispName = "Contacto Visita"
    !!@ ili2db.dispName="Contacto Visita"
    CLASS DLC_ContactoVisita =
      /** Indica el tipo de documento de la persona que atendió la visita predial.
       */
      !!@ ili2db.dispName = "Tipo de documento de quien atendió la visita predial"
      !!@ ili2db.dispName="Tipo de documento de quien atendió la visita predial"
      Tipo_Documento_Quien_Atendio : MANDATORY Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_InteresadoDocumentoTipo;
      /** Indica el número de documento de la persona que atendió la visita predial.
       */
      !!@ ili2db.dispName = "Numero de documento de quien atendió la visita predial"
      !!@ ili2db.dispName="Numero de documento de quien atendió la visita predial"
      Numero_Documento_Quien_Atendio : MANDATORY TEXT*50;
      /** Primer nombre de la persona encargada de atender la visita de ejecución de actividades catastrales sobre el predio.
       */
      !!@ ili2db.dispName = "Primer nombre de quien atendió"
      !!@ ili2db.dispName="Primer nombre de quien atendió"
      Primer_Nombre_Quien_Atendio : MANDATORY TEXT*100;
      /** Segundo nombre de la persona encargada de atender la visita de ejecución de actividades catastrales sobre el predio.
       */
      !!@ ili2db.dispName = "Segundo nombre de quien atendió"
      !!@ ili2db.dispName="Segundo nombre de quien atendió"
      Segundo_Nombre_Quien_Atendio : TEXT*100;
      /** Primer apellido de la persona encargada de atender la visita de ejecución de actividades catastrales sobre el predio.
       */
      !!@ ili2db.dispName = "Primer apellido de quien atendió"
      !!@ ili2db.dispName="Primer apellido de quien atendió"
      Primer_Apellido_Quien_Atendio : MANDATORY TEXT*100;
      /** Segundo apellido de la persona encargada de atender la visita de ejecución de actividades catastrales sobre el predio.
       */
      !!@ ili2db.dispName = "Segundo apellido de quien atendió"
      !!@ ili2db.dispName="Segundo apellido de quien atendió"
      Segundo_Apellido_Quien_Atendio : TEXT*100;
      /** Relación con el predio de la persona encargada de atender la visita, por ejemplo propietario, encargado, administrador, poseedor, etc.
       */
      !!@ ili2db.dispName = "Relación con el predio"
      !!@ ili2db.dispName="Relación con el predio"
      Relacion_Con_Predio : MANDATORY Submodelo_Datos_Levantamiento_Catastral_V1_0.DLC_RelacionPredioTipo;
      /** Dirección de domicilio para notificaciones.
       */
      !!@ ili2db.dispName = "Domicilio notificaciones"
      !!@ ili2db.dispName="Domicilio notificaciones"
      Domicilio_Notificaciones : TEXT*255;
      /** Número de celular de la persona que atendió la visita de actividades catastrales.
       */
      !!@ ili2db.dispName = "Celular"
      !!@ ili2db.dispName="Celular"
      Celular : TEXT*20;
      /** Correo electrónico de la persona que atendió la visita de actividades catastrales.
       */
      !!@ ili2db.dispName = "Correo electrónico"
      !!@ ili2db.dispName="Correo electrónico"
      Correo_Electronico : TEXT*100;
      /** Indica si autoriza las notificaciones administrativas por medio de vía electrónica.
       */
      !!@ ili2db.dispName = "Autoriza notificaciones"
      !!@ ili2db.dispName="Autoriza notificaciones"
      Autoriza_Notificaciones : BOOLEAN;
    END DLC_ContactoVisita;

    !!@ ili2db.dispName = "(LC) Estructura Novedad FMI"
    !!@ ili2db.dispName="(LC) Estructura Novedad FMI"
    STRUCTURE DLC_EstructuraNovedadFMI =
      !!@ ili2db.dispName = "Código ORIP"
      !!@ ili2db.dispName="Código ORIP"
      Codigo_ORIP : TEXT*3;
      !!@ ili2db.dispName = "Número FMI"
      !!@ ili2db.dispName="Número FMI"
      Numero_FMI : TEXT*80;
    END DLC_EstructuraNovedadFMI;

    /** Datos de contacto de la parte interesada.
     */
    !!@ ili2db.dispName = "Interesado Contacto"
    !!@ ili2db.dispName="Interesado Contacto"
    CLASS DLC_InteresadoContacto =
      /** Numero de telefono de contacto del interesado.
       */
      !!@ ili2db.dispName = "Teléfono 1"
      !!@ ili2db.dispName="Teléfono 1"
      Telefono1 : TEXT*20;
      /** Numero de telefono de contacto del interesado.
       */
      !!@ ili2db.dispName = "Teléfono 2"
      !!@ ili2db.dispName="Teléfono 2"
      Telefono2 : TEXT*20;
      /** Domicilio para notificaciones del interesado.
       */
      !!@ ili2db.dispName = "Domicilio notificación"
      !!@ ili2db.dispName="Domicilio notificación"
      Domicilio_Notificacion : TEXT*500;
      /** Dirección de residencia del interesado.
       */
      !!@ ili2db.dispName = "Dirección de residencia"
      !!@ ili2db.dispName="Dirección de residencia"
      Direccion_Residencia : TEXT*500;
      /** Correo electrónico del interesado.
       */
      !!@ ili2db.dispName = "Correo electrónico"
      !!@ ili2db.dispName="Correo electrónico"
      Correo_Electronico : TEXT*100;
      /** Indica si el interesado autoriza notificación vía correo electrónico
       */
      !!@ ili2db.dispName = "Autoriza notificación correo"
      !!@ ili2db.dispName="Autoriza notificación correo"
      Autoriza_Notificacion_Correo : BOOLEAN;
      /** Departamento de residencia del interesado.
       */
      !!@ ili2db.dispName = "Departamento"
      !!@ ili2db.dispName="Departamento"
      Departamento : MANDATORY TEXT*100;
      /** Municipio de residencia del interesado.
       */
      !!@ ili2db.dispName = "Municipio"
      !!@ ili2db.dispName="Municipio"
      Municipio : MANDATORY TEXT*100;
      /** Vereda de residencia del interesado.
       */
      !!@ ili2db.dispName = "Vereda"
      !!@ ili2db.dispName="Vereda"
      Vereda : TEXT*100;
      /** Corregimiento de residencia del interesado.
       */
      !!@ ili2db.dispName = "Corregimiento"
      !!@ ili2db.dispName="Corregimiento"
      Corregimiento : TEXT*100;
    END DLC_InteresadoContacto;

    !!@ ili2db.dispName = "(LC) Estructura Novedad Número Predial"
    !!@ ili2db.dispName="(LC) Estructura Novedad Número Predial"
    STRUCTURE DLC_EstructuraNovedadNumeroPredial =
      !!@ ili2db.dispName = "Número predial"
      !!@ ili2db.dispName="Número predial"
      Numero_Predial : MANDATORY TEXT*30;
      !!@ ili2db.dispName = "Tipo de novedad"
      !!@ ili2db.dispName="Tipo de novedad"
      Tipo_Novedad : (
        !!@ ili2db.dispName = "Predio nuevo"
        !!@ ili2db.dispName="Predio nuevo"
        Predio_Nuevo,
        !!@ ili2db.dispName = "Desenglobe"
        !!@ ili2db.dispName="Desenglobe"
        Desenglobe,
        !!@ ili2db.dispName = "Englobe"
        !!@ ili2db.dispName="Englobe"
        Englobe,
        !!@ ili2db.dispName = "Cancelación"
        !!@ ili2db.dispName="Cancelación"
        Cancelacion,
        !!@ ili2db.dispName = "Cambio de número predial"
        !!@ ili2db.dispName="Cambio de número predial"
        Cambio_Numero_Predial,
        !!@ ili2db.dispName = "Ninguna"
        !!@ ili2db.dispName="Ninguna"
        Ninguna
      );
    END DLC_EstructuraNovedadNumeroPredial;

    ASSOCIATION dlc_interesado_interesadocontacto =
      dlc_contacto -- {0..*} DLC_InteresadoContacto;
      sinic_interesado (EXTERNAL) -- {1} Modelo_Aplicacion_LADMCOL_SINIC_V1_0.Sistema_Nacional_Informacion_Catastral.SINIC_Interesado;
    END dlc_interesado_interesadocontacto;

    /** Datos de adicionales del predio para catastro multipropósito
     */
    !!@ ili2db.dispName = "Datos adicionales levantamiento catastral"
    !!@ ili2db.dispName="Datos adicionales levantamiento catastral"
    CLASS DLC_DatosAdicionalesLevantamientoCatastral =
      /** Indica si el predio según la información registral cuenta con el valor de área en la cabida y linderos.
       */
      !!@ ili2db.dispName = "¿El predio tiene área registral?"
      !!@ ili2db.dispName="¿El predio tiene área registral?"
      Tiene_Area_Registral : MANDATORY BOOLEAN;
      /** Indica el valor del área registral del predio en metros cuadrados.
       */
      !!@ ili2db.dispName = "Área registral en metros cuadrados"
      !!@ ili2db.dispName="Área registral en metros cuadrados"
      Area_Registral_M2 : 0.00 .. 10000000000000000000000.00;
      /** Se considerará como procedimientos catastrales con efectos registrales los siguientes: actualización de linderos, rectificación de área por imprecisa determinación, actualización masiva y puntual de linderos y áreas, rectificación de linderos por acuerdo entre las partes, e inclusión de área y/o linderos.
       */
      !!@ ili2db.dispName = "Procedimientos catastrales con efectos registrales"
      !!@ ili2db.dispName="Procedimientos catastrales con efectos registrales"
      Procedimiento_Catastral_Registral : MANDATORY Submodelo_Datos_Levantamiento_Catastral_V1_0.DLC_ProcedimientoCatastralRegistralTipo;
      /** Relaciona los cambios del predio respecto de la información catastral vigente.
       */
      !!@ ili2db.dispName = "Novedad números prediales"
      !!@ ili2db.dispName="Novedad números prediales"
      Novedad_Numeros_Prediales : BAG {0..*} OF Submodelo_Datos_Levantamiento_Catastral_V1_0.Datos_Levantamiento_Catastral.DLC_EstructuraNovedadNumeroPredial;
      /** Relaciona los cambios del predio respecto de la información registral vigente.
       */
      !!@ ili2db.dispName = "Novedad folio de matrícula de inmobiliaria"
      !!@ ili2db.dispName="Novedad folio de matrícula de inmobiliaria"
      Novedad_FMI : BAG {0..*} OF Submodelo_Datos_Levantamiento_Catastral_V1_0.Datos_Levantamiento_Catastral.DLC_EstructuraNovedadFMI;
      /** Observaciones generales respecto del predio.
       */
      !!@ ili2db.dispName = "Observaciones"
      !!@ ili2db.dispName="Observaciones"
      Observaciones : MTEXT*500;
      /** Fecha de la visita en campo al predio.
       */
      !!@ ili2db.dispName = "Fecha de visita predial"
      !!@ ili2db.dispName="Fecha de visita predial"
      Fecha_Visita_predial : MANDATORY INTERLIS.XMLDate;
      /** Indica el tipo de documento del reconocedor predial encargado de efectuar el levantamiento catastral.
       */
      !!@ ili2db.dispName = "Tipo de documento del reconocedor predial"
      !!@ ili2db.dispName="Tipo de documento del reconocedor predial"
      Tipo_Documento_Reconocedor : MANDATORY Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_InteresadoDocumentoTipo;
      /** Indica el número de documento del reconocedor predial encargado de efectuar el levantamiento catastral.
       */
      !!@ ili2db.dispName = "Numero de documento del reconocedor predial"
      !!@ ili2db.dispName="Numero de documento del reconocedor predial"
      Numero_Documento_Reconocedor : MANDATORY TEXT*50;
      /** Primer nombre del delegado de ejecutar las actividades catastrales en campo en el predio.
       */
      !!@ ili2db.dispName = "Primer nombre del reconocedor"
      !!@ ili2db.dispName="Primer nombre del reconocedor"
      Primer_Nombre_Reconocedor : MANDATORY TEXT*100;
      /** Segundo nombre del delegado de ejecutar las actividades catastrales en campo en el predio.
       */
      !!@ ili2db.dispName = "Segundo nombre del reconocedor"
      !!@ ili2db.dispName="Segundo nombre del reconocedor"
      Segundo_Nombre_Reconocedor : TEXT*100;
      /** Primer apellido del delegado de ejecutar las actividades catastrales en campo en el predio.
       */
      !!@ ili2db.dispName = "Primer apellido del reconocedor"
      !!@ ili2db.dispName="Primer apellido del reconocedor"
      Primer_Apellido_Reconocedor : MANDATORY TEXT*100;
      /** Segundo apellido del delegado de ejecutar las actividades catastrales en campo en el predio.
       */
      !!@ ili2db.dispName = "Segundo apellido del reconocedor"
      !!@ ili2db.dispName="Segundo apellido del reconocedor"
      Segundo_Apellido_Reconocedor : TEXT*100;
      /** Posibles situaciones que se presentan y afectan el resultado del levantamiento de la información en campo.
       */
      !!@ ili2db.dispName = "Resultado de la visita predial"
      !!@ ili2db.dispName="Resultado de la visita predial"
      Resultado_Visita : MANDATORY Submodelo_Datos_Levantamiento_Catastral_V1_0.DLC_ResultadoVisitaTipo;
      /** Indica si dentro del proceso de formación o actualización catastral se suscribe acta de colindancia para el predio objeto de levantamiento catastral.
       */
      !!@ ili2db.dispName = "¿Se suscribe acta de colindancia?"
      !!@ ili2db.dispName="¿Se suscribe acta de colindancia?"
      Suscribe_Acta_Colindancia : BOOLEAN;
    END DLC_DatosAdicionalesLevantamientoCatastral;

    ASSOCIATION dlc_informacion_adicional_contacto =
      dlc_contacto_visita -- {0..*} DLC_ContactoVisita;
      dlc_datos_adicionales -- {1} DLC_DatosAdicionalesLevantamientoCatastral;
    END dlc_informacion_adicional_contacto;

    /** Relación entre el Predio catastral y la clase información adicional, que permite determinar sobre qué predio se toman los datos específicos del predio para catastro multipropósito
     */
    ASSOCIATION dlc_informacion_adicional_predio =
      sinic_predio (EXTERNAL) -- {1} Modelo_Aplicacion_LADMCOL_SINIC_V1_0.Sistema_Nacional_Informacion_Catastral.SINIC_Predio;
      dlc_datos_adicionales -- {0..1} DLC_DatosAdicionalesLevantamientoCatastral;
    END dlc_informacion_adicional_predio;

  END Datos_Levantamiento_Catastral;

END Submodelo_Datos_Levantamiento_Catastral_V1_0.
