      !!@ ili2db.dispName = "¿Cuál es el tipo de derecho que tiene sobre el (los) predios(s)?"
      Relacion_Otros_Predios : Submodelo_FISO_V1_0.FISO_ExplotaOtroPredio;
      /** Especifica la relación que hay sobre la existencia de otro predio diferente a las opciones dadas.
       */
      !!@ ili2db.dispName = "Otro tipo de relación con otros predios"
      Otro_Tipo_Relacion : TEXT*255;
      /** Define los atributos de tradición del predio respecto de la relación jurídica con el solicitante o aspirante en FISO.
       */
      !!@ ili2db.dispName = "¿Cuál de las siguientes opciones es la que usted considera que le otorga algún tipo de derecho?"
      Opcion_Otorga_Derecho : Submodelo_FISO_V1_0.FISO_OtorgaDerechoTipo;
      /** Indaga por el tipo de relación diferente a las opciones predeterminadas dentro del formulario.
       */
      !!@ ili2db.dispName = "¿Cúal otra opción le otorga el derecho?"
      Otra_Opcion_Otorga_Derecho : TEXT*100;
      /** Documentos que soportan su relación con el predio.
       */
      !!@ ili2db.dispName = "Documentos de soporte"
      Documentos_Soporte : BAG {0..*} OF LADM_COL_V3_0.LADM_Nucleo.ExtArchivo;
      /** Indaga por los datos de contacto de la persona de la cual obtuvo el predio objeto de la solicitud.
       */
      !!@ ili2db.dispName = "¿Tiene los datos de contacto de la persona de la cual obtuvo el predio?"
      Tiene_Datos_Persona_Obtuvo_Predio : MANDATORY BOOLEAN;
    END FISO_RelacionJuridicaSolicitantePredio;

