      !!@ ili2db.dispName = "Es una donación"
      Donacion,
      /** Demás relaciones de derechos sobre los predios que una persona pueda ostentar y  no se encuentren predeterminadas en las opciones de respuesta
       */
      !!@ ili2db.dispName = "Otro"
