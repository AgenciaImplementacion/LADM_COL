    /** Define los conflictos asociados a la tenencia de la tierra
     */
    !!@ ili2db.dispName = "(FADC) Conflictos"
    CLASS FADC_Conflictos
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.ObjetoVersionado =
      /** En este campo se eligen las categorías y subcategorías de conflictos que se quieren asociar a un predio.
       */
      !!@ ili2db.dispName = "Tipo de conflicto"
      Tipo_Conflicto : MANDATORY Submodelo_FADC_V1_0.FADC_ConflictosTipo;
