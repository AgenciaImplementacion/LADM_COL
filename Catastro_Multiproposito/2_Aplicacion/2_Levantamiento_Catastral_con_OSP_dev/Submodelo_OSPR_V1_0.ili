  TOPIC FISO =
    OID AS INTERLIS.UUIDOID;
    DEPENDS ON Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral,Submodelo_ANT_V1_0.ANT;

    /** Define los atributos que identifican si el aspirante o solicitante de FISO ha recibido beneficios de programa de tierras
     */
    !!@ ili2db.dispName = "(FISO) Beneficios del programa de tierras"
    CLASS FISO_BeneficiosProgramaTierras
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.ObjetoVersionado =
      /** El solicitante informa si en el pasado ha recibido beneficios de las entidades INCORA, INCODER.
       */
      !!@ ili2db.dispName = "¿Ha sido beneficiario(a) de tierras por parte del INCORA, INCODER, ANT?"
      Beneficiario_Tierra : MANDATORY BOOLEAN;
      /** El solicitante que fue beneficiario en el pasado informa si tiene documento que compruebe el beneficio
       */
      !!@ ili2db.dispName = "¿Cuenta con algún documento que acredita el beneficio?"
      Documento_Acredita_Beneficio : BOOLEAN;
      /** Número del acto administrativo con el cual se otorgó el beneficio del programa de tierras.
       */
      !!@ ili2db.dispName = "Número de resolución"
      Numero_Resolucion : TEXT*50;
      /** Fecha de la resolución del acto administrativo.
       */
      !!@ ili2db.dispName = "Fecha de resolución"
      Fecha_Resolucion : INTERLIS.XMLDate;
    END FISO_BeneficiosProgramaTierras;

    !!@ ili2db.dispName = "(FISO) Estructura actividades"
    STRUCTURE FISO_EstructuraActividades =
      /** Tipo de actividad en la cual tiene experiencia
       */
      !!@ ili2db.dispName = "Tipo de actividad"
      Tipo_Actividad : MANDATORY Submodelo_FISO_V1_0.FISO_ActividadTipo;
      /** Tipo de actividad en la cual tiene experiencia
       */
      !!@ ili2db.dispName = "Otro tipo de actividad"
      Otro_Tipo_Actividad : TEXT*100;
      /** Meses de experiencia en la actividad
       */
      !!@ ili2db.dispName = "Meses de experiencia"
      Meses_Experiencia : MANDATORY 0 .. 999999999;
    END FISO_EstructuraActividades;

    /** Estructura que permite almacenar el tipo de explotación del predio de acuerdo al formulario de FISO.
     */
    !!@ ili2db.dispName = "(FISO) Estructura clase de explotación del predio"
    STRUCTURE FISO_EstructuraClaseExplotacion =
      /** Uso o destinación que el solicitante le da al predio objeto de la solicitud.
       */
      !!@ ili2db.dispName = "Clase de explotación"
      Clase_Explotacion : MANDATORY Submodelo_FISO_V1_0.FISO_ClaseExplotacionTipo;
      /** Uso o destinación que el solicitante le da al predio objeto de la solicitud que no esta dentro de las opciones predeterminadas en el formulario.
       */
      !!@ ili2db.dispName = "Otra clase de explotación"
      Otra_Clase_Explotacion : TEXT;
    END FISO_EstructuraClaseExplotacion;

    /** Estructura para el almacenamiento de conflictos existentes en el predio para el formulario de FISO.
     */
    !!@ ili2db.dispName = "(FISO) Estructura diligenciamiento de conflictos"
    STRUCTURE FISO_EstructuraConflictos =
      /** Indaga por la existencia de conflictos de algún tipo que puedan interferir en la legalización del predio objeto de la solicitud.
       */
      !!@ ili2db.dispName = "Tipo de conflicto"
      Tipo_Conflicto : MANDATORY Submodelo_FISO_V1_0.FISO_ConflictosTipo;
      /** Indaga sobre otro tipo de conflicto que no existe sobre el predio objeto de la solicitud.
       */
      !!@ ili2db.dispName = "Otro tipo de conflictos"
      Otro_Tipo_Conflicto : TEXT;
    END FISO_EstructuraConflictos;

    /** Estructura que permite almacenar las limitantes de una persona según el formulario de FISO.
     */
    !!@ ili2db.dispName = "(FISO) Estructura limitantes"
    STRUCTURE FISO_EstructuraLimitantes =
      /** Relaciona el tipo de limitación que impide a la persona desarrollar sus actividades normales.
       */
      !!@ ili2db.dispName = "Tipo de limitantes"
      Tipo_Limitantes : MANDATORY Submodelo_ANT_V1_0.ANT_LimitantePermanenteTipo;
      /** Nombre de la limitación que no esta dentro de las opciones dadas.
       */
      !!@ ili2db.dispName = "Otro tipo de limitantes"
      Otro_Tipo_Limitantes : TEXT*100;
    END FISO_EstructuraLimitantes;

    /** Estructura que permite almacenar las ocupaciones de una actividad laboral para una persona según el formulario de FISO.
     */
    !!@ ili2db.dispName = "(FISO) Estructura ocupación"
    STRUCTURE FISO_EstructuraOcupacion =
      /** Oficio o labor desarrollada por el solicitante entre las opciones dadas.
