      Tipo_Entrada_FISO : MANDATORY Submodelo_FISO_V1_0.FISO_EntradaFISOTipo;
      /** ID barrido ANT o QR del predio.
       */
      !!@ ili2db.dispName = "ID Barrido ANT(QR)"
      ID_Barrido_ANT_QR : TEXT*10;
      /** UGT u Operador que realiza el diligenciamiento del FISO.
       */
      !!@ ili2db.dispName = "Responsable de diligenciamiento de la solicitud"
      Responsable_Solicitud : MANDATORY Submodelo_FISO_V1_0.FISO_ResponsableSolicitudTipo;
      /** Nombre del operador que hace el diligenciamiento del FISO.
       */
      !!@ ili2db.dispName = "Otro responsable de diligenciamiento de la solicitud"
      Otro_Responsable : TEXT*100;
      /** Refiere la ruta jurídica que debe seguir la solicitud.
       */
      !!@ ili2db.dispName = "Tipo de solicitud"
      Tipo_Solicitud : MANDATORY Submodelo_FISO_V1_0.FISO_SolicitudTipo;
      /** Refiere si la solicitud la presenta directamente el ciudadano o su apoderado.
       */
      !!@ ili2db.dispName = "¿La solicitud la presenta directamente el solicitante o por medio de apoderado?"
      Quien_Presenta_Solicitud : MANDATORY Submodelo_FISO_V1_0.FISO_PresentaSolicitudTipo;
      /** Por favor registre si la persona que diligencia el FISO sabe firmar.
       */
      !!@ ili2db.dispName = "Registre si la persona que diligencia el FISO sabe firmar"
      Persona_Diligencia_Sabe_Firmar : MANDATORY BOOLEAN;
      /** Autorización para que el solicitante permita consultar las bases de datos de la DIAN bajo el artículo 584 del estatuto tributario Decreto 624 de 1989.
       */
      !!@ ili2db.dispName = "Bajo la gravedad de juramento declaro que la información personal suministrada es verdadera y autorizo que sea verificada con otras fuentes de información y que se actualice de forma automática a través del cruce de registros administrativos y consultas de información a la DIAN, de acuerdo con lo dispuesto en el artículo 584 del estatuto tributario Decreto 624 de 1989: ''Examen de la declaración con autorización del declarante'', u otrás fuentes que la ANT defina. Cualquier presunta falsedad identificada a través de cruces de bases de datos generará la exclusión del RESO y no podrá ingresar en un período de 10 años , independiente de las acciones legales que haya lugar. (Artículo 15 del Decreto 902 de 2017)"
      Juramento_Declaracion_Informacion : MANDATORY BOOLEAN;
      /** Autorización del solicitante para recibir mensajes de texto con información del proceso.
       */
      !!@ ili2db.dispName = "Acepta recibir mensajes a través del correo electrónico del estado y/o avance de su proceso y ser notificado por dicho medio electrónico de las actuaciones que adelanta la ANT"
      Acepta_Recibir_Mensajes : MANDATORY BOOLEAN;
      /** Autorización para que el conyugue del  solicitante permita consultar las bases de datos de la DIAN bajo el articulo 584 del estatuto tributario Decreto 624 de 1989.
       */
      !!@ ili2db.dispName = "En virtud de los principios de planeación y eficiencia se precisa que en caso de que el (la) cónyuge no pueda ser valorado(a) al momento de analizar la presente solicitud – FISO, conforme al artículo 25 del Decreto Ley 902 de 2017 la titulación se realizará de manera conjunta, para ello se entenderá que con el solo diligenciamiento de este documento por parte suya o de su cónyuge o compañero(a) permanente será posible aplicar un esquema de arrastre tanto de puntajes como de información reportada en el formulario, el cual no requerirá de un nuevo FISO. Ello sin perjuicio de la obligación de laANT de verificar en VUR, DIAN y demás bases de datos la existencia de alguna prohibición que impida la adjudicación."
      Autorizacion_Conyuge : BOOLEAN;
      /** Se consigna la ampliación de la información que no se puede registrar con las preguntas predeterminadas del formulario.
       */
      !!@ ili2db.dispName = "Observaciones"
      Observaciones : TEXT;
      /** Ubicación geográfica donde se captura el formulario FISO.
       */
      !!@ ili2db.dispName = "Geometría"
      Geometria : ISO19107_PLANAS_V3_0.GM_Point3D;
      /** Versión del Formulario FISO.
       */
      !!@ ili2db.dispName = "Versión FISO"
      Version_FISO : TEXT*20;
    END FISO_IdentificacionSolicitud;

    /** Define los atributos de caracterización del solicitante o aspirante en FISO
     */
