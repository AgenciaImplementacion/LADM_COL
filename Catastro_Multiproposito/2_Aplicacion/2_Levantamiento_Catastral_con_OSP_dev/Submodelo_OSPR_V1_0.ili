INTERLIS 2.3;

MODEL Submodelo_ANT_V1_0 (es)
AT "mailto:JONNY@localhost"
VERSION "2020-10-20"  =
  IMPORTS LADM_COL_V3_0,Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0;

  DOMAIN

    ANT_EstadoFMITipo = (
      /** Corresponden a todos los folios que se encuentran activos y no han sido cerrados.
       */
      !!@ ili2db.dispName = "Activo"
      Activo,
      /** Siempre que se engloben varios predios o la venta de la parte restante de ellos o se cancelen por orden judicial o administrativa los títulos o documentos que la sustentan jurídicamente y no existan anotaciones vigentes, las matrículas inmobiliarias se cerrarán para el efecto o se hará una anotación que diga "Folio Cerrado".
       */
      !!@ ili2db.dispName = "Cerrado"
      Cerrado,
      /** Corresponden a todos los folios que se encuentran trasladados.
       */
      !!@ ili2db.dispName = "Trasladado"
      Trasladado
    );

    ANT_LimitantePermanenteTipo = (
      /** Percibir algo material por medio del sentido de la vista
       */
      !!@ ili2db.dispName = "Ver"
      Ver,
      /** Término que refiere a la facultad sensorial para lograr la percepción de un sonido
       */
      !!@ ili2db.dispName = "Oir"
      Oir,
      /** Hablar es el acto en el que un individuo intenta comunicarse, usando una lengua o idioma, siendo esto, básicamente, la vocalización de sonidos.
       */
      !!@ ili2db.dispName = "Hablar"
      Hablar,
      /** Entiendase como la capacidad que tiene una persona para moverse o camniar una persona sin ayuda.
       */
      !!@ ili2db.dispName = "Moverse o caminar por si mismo"
      Moverse_Caminar,
      /** Entiendase como la capacidad que tiene una persona para bañarse, vestirse o alimentarse una persona sin ayuda.
       */
      !!@ ili2db.dispName = "Bañarse, vestirse o alimentarse por si mismo"
      Baniarse_Vestirse_Alimentarse,
      /** Persona que presenta problemas de moviemiento u otras carasteristicas que no permiten su salida a la calle sin ayuda de otra persona repsonsable.
       */
      !!@ ili2db.dispName = "Dificultad para salir a la calle sin ayuda o compañía"
      Dificultad_Salir_Calle,
      /** Acción de adquirir y retener conocimiento, habilidad o información sobre alguna materia o tema.
       */
      !!@ ili2db.dispName = "Entender o aprender"
      Entender_Aprender,
      /** No esta dentro de las opciones brindadas
       */
      !!@ ili2db.dispName = "Otro"
      Otra
    );

    ANT_OcupacionTipo = (
      /** Persona que trabaja para otra o para una institución/empresa a cambio de un salario.
       */
      !!@ ili2db.dispName = "Empleado"
      Empleado,
      /** Aquellas personas que no están vinculadas a una empresa mediante un contrato de trabajo, sino mediante un contrato de servicios y son remunerados bajo la figura de honorarios o comisiones.
       */
      !!@ ili2db.dispName = "Independiente"
      Independiente,
      /** Persona que realiza las actividades de limpiar, barrer, hacer reparaciones menores en la vivienda, cuidar animales domésticos o el jardín.
       */
      !!@ ili2db.dispName = "Oficios del hogar"
      Oficios_Hogar,
      /** Persona productora que vive de la producción para el autoconsumo y que, en caso de tener excedentes, los puede comercializar, sin ser esta su finalidad.
       */
      !!@ ili2db.dispName = "Campesino"
      Campesino,
      /** Es toda persona que tiene tradición en labores rurales y que obtiene principalmente ingresos provenientes de actividades agrícolas, pecuarias, acuícolas, piscícolas o forestales. Puede o no ser propietaria de la tierra.
       */
      !!@ ili2db.dispName = "Trabajador agrario"
      Trabajador_Agrario,
      /** Persona que no puede ejercer ninguna actividad incapacidad total o parcial
       */
      !!@ ili2db.dispName = "Incapacitado permanente para trabajar"
      Incapacitado_Permanente_Trabajar,
      /** No esta dentro de las opciones brindadas
       */
      !!@ ili2db.dispName = "Otro"
      Otro
    );

    ANT_ParentescoTipo = (
      /** Persona casada con vinculo matrimonial civil o religioso  vigente, con relación a su cónyuge.
       */
      !!@ ili2db.dispName = "Esposo/Esposa"
      Esposo_Esposa,
      /** Persona que sin estar casada, hizo una comunidad de vida permanente y singular con otra, sea de su mismo sexo o diferente
       */
      !!@ ili2db.dispName = "Compañero/Compañera permanente"
      Companiero_Companiera_Permanente,
      /** Relación por consanguinidad o legal que una persona tiene con respecto a sus padres
       */
      !!@ ili2db.dispName = "Hijo/Hija"
      Hijo_Hija,
      /** Hijo aportado al nuevo matrimonio por el cónyuge o compañero(a) permanente de una persona
       */
      !!@ ili2db.dispName = "Hijastro/Hijastra"
      Hijastro_Hijastra,
      /** Persona que con respecto a otra llamada padre o madre de crianza, ocupa el lugar de un hijo con motivo de un lazo afectivo que los une, sin que medie vínculo de consanguinidad o civil que genere derechos u obligaciones.
       */
      !!@ ili2db.dispName = "Hijo/Hija de crianza"
      Hijo_Hija_Crianza,
      /** Relación por consanguinidad o legal que una persona tiene con respecto a sus hijos
       */
      !!@ ili2db.dispName = "Padre/Madre"
      Padre_Madre,
      /** Relación que una persona tiene con respecto al Padre o madre del padre o de la madre.
       */
      !!@ ili2db.dispName = "Abuelo/Abuela"
      Abuelo_Abuela,
      /** Vinculo que tiene un hijo con los hijos de los mismos padres, o del mismo padre o madre.
       */
      !!@ ili2db.dispName = "Hermano/Hermana"
      Hermano_Hermana,
      /** Padre o madre de uno de los esposos o compañero permanente respecto del otro.
       */
      !!@ ili2db.dispName = "Suegro/Suegra"
      Suegro_Suegra,
      /** Relación que tiene un padre con el esposo de su hija.
       */
      !!@ ili2db.dispName = "Yerno/Nuera"
      Yerno_Nuera,
      /** Relación que tiene un(a) hermano(a) con el esposo de su hermano(a).
       */
      !!@ ili2db.dispName = "Cuñado/Cuñada"
      Cuniado_Cuniada,
      /** Padre o madre de uno de los abuelos de una persona.
       */
      !!@ ili2db.dispName = "Bisabuelo/Bisabuela"
      Bisabuelo_Bisabuela,
      /** Relación que tiene el hijo de una persona con el o los Hermanos de uno de los padres.
       */
      !!@ ili2db.dispName = "Tío/Tía"
      Tio_Tia,
      /** Relación que tiene una persona con los hijos de sus hermanos
       */
      !!@ ili2db.dispName = "Sobrino/Sobrina"
      Sobrino_Sobrina,
      /** Relación que tiene una persona respecto de los hijos de sus hijos
       */
      !!@ ili2db.dispName = "Nieto/Nieta"
      Nieto_Nieta,
      /** Hijo del nieto de una persona
       */
      !!@ ili2db.dispName = "Biznieto/Biznieta"
      Biznieto_Biznieta,
      /** Persona que se halla en el cuarto grado de parentesco colateral, con respecto a otro, donde uno de sus padres es hermano de uno de los padres del otro, que son a su vez tíos de cada uno de ellos.
       */
      !!@ ili2db.dispName = "Primo/Prima"
      Primo_Prima,
      /** Parentesco o relación de afinidad distinta a las predeterminadas en la opciones de respuesta
       */
      !!@ ili2db.dispName = "Otro"
      Otro
    );

    ANT_RespuestaTipo = (
      /** Está de acuerdo con la pregunta
       */
      !!@ ili2db.dispName = "Si"
      Si,
      /** No está de acuerdo con la pregunta
       */
      !!@ ili2db.dispName = "No"
      No,
      /** No sabe o no responde
       */
      !!@ ili2db.dispName = "No sabe / No responde"
      No_Sabe_No_Responde
    );

  TOPIC ANT =
    DEPENDS ON Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral;

    /** Estructura que permite el almacenamiento de las matrículas inmobiliarias matrices de un predio en específico.
     */
    !!@ ili2db.dispName = "(ANT) Estructura FMI Matriz"
    STRUCTURE ANT_EstructuraFMIMatriz =
      /** Círculo registral del folio de matrícula inmobiliaria del predio matriz.
       */
      !!@ ili2db.dispName = "FMI Matriz: Círculo Registral"
      Codigo_ORIP : TEXT*3;
      /** Número de matrícula inmobiliaria del folio de matrícula inmobiliaria del predio matriz.
       */
      !!@ ili2db.dispName = "FMI Matriz: Matrícula inmobiliaria"
      Matricula_Inmobiliaria : MANDATORY TEXT*80;
      /** Responde a la situación registral de un Folio de Matrícula Inmobiliaria que ha tenido mutaciones históricas, principalmente desenglobes. Debe tener al menos un FMI derivado y se presenta como ACTIVO o CERRADO, éste último si de las mutaciones históricas o desenglobes presentados, ya no existe área remanente que lo mantenga vivo jurídicamente.
       */
      !!@ ili2db.dispName = "Estado FMI Matriz"
      Estado_FMI : Submodelo_ANT_V1_0.ANT_EstadoFMITipo;
    END ANT_EstructuraFMIMatriz;

    /** Estructura que me permite almacenar los registros de los QR históricos para un predio.
     */
    !!@ ili2db.dispName = "(ANT) Estructura QR Anterior"
    STRUCTURE ANT_EstructuraQRAnterior =
      /** QR anterior(es) relacionados con el predio.
       */
      !!@ ili2db.dispName = "QR Anterior"
      QR_Anterior : MANDATORY TEXT*10;
    END ANT_EstructuraQRAnterior;

    /** Datos de personas diferentes a los que tienen relación de derecho directa con el predio.
     */
    !!@ ili2db.dispName = "(ANT) Persona"
    CLASS ANT_Persona
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.ObjetoVersionado =
      /** Tipo de persona natural o jurídica.
       */
      !!@ ili2db.dispName = "Tipo"
      Tipo : Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.LC_InteresadoTipo;
      /** Tipo de documento de identificación de interesado.
       */
      !!@ ili2db.dispName = "Tipo de documento"
      Tipo_Documento : Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.LC_InteresadoDocumentoTipo;
      /** Número de documento de identidad del interesado.
       */
      !!@ ili2db.dispName = "Documento de identidad"
      Documento_Identidad : TEXT*50;
      /** Primer nombre de la persona física.
       */
      !!@ ili2db.dispName = "Primer nombre"
      Primer_Nombre : TEXT*100;
      /** Segundo nombre de la persona física.
       */
      !!@ ili2db.dispName = "Segundo nombre"
      Segundo_Nombre : TEXT*100;
      /** Primer apellido de la persona física.
       */
      !!@ ili2db.dispName = "Primer apellido"
      Primer_Apellido : TEXT*100;
      /** Segundo apellido de la persona física.
       */
      !!@ ili2db.dispName = "Segundo apellido"
      Segundo_Apellido : TEXT*100;
      /** Sexo del interesado persona natural.
       */
      !!@ ili2db.dispName = "Sexo"
      Sexo : Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.LC_SexoTipo;
      /** Grupo étnico del interesado persona natural.
       */
      !!@ ili2db.dispName = "Grupo étnico"
      Grupo_Etnico : Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.LC_GrupoEtnicoTipo;
      /** Nombre con que una entidad (pública o privada) o sociedad mercantil está registrada legalmente
       */
      !!@ ili2db.dispName = "Razón social"
      Razon_Social : TEXT*255;
      /** Otros datos de la persona entre ellos: huella dactilar, firma, fotografía.
       */
      !!@ ili2db.dispName = "Otros datos"
      Otros_Datos : LADM_COL_V3_0.LADM_Nucleo.ExtInteresado;
    END ANT_Persona;

    /** Datos de caracterización de la persona que se registra.
     */
    !!@ ili2db.dispName = "(ANT) Persona caracterización"
    CLASS ANT_PersonaCaracterizacion
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.ObjetoVersionado =
      Fecha_Nacimiento : TEXT*20;
      /** Parentesco del familiar con el aspirante o solicitante.
       */
      !!@ ili2db.dispName = "Parentesco"
      Parentesco : Submodelo_ANT_V1_0.ANT_ParentescoTipo;
      /** Identificación de las limitantes permanentes del familiar del aspirante o solicitante.
       */
      !!@ ili2db.dispName = "¿Tiene limitantes permanentes?"
      Limitantes_Permanentes : Submodelo_ANT_V1_0.ANT_LimitantePermanenteTipo;
      /** Otro tipo de limitantes del familiar del aspirante o solicitante.
       */
      !!@ ili2db.dispName = "Otra limitante permanente"
      Otra_Limitante_Permanente : TEXT*150;
      /** Ocupación actual del familiar del aspirante o solicitante.
       */
      !!@ ili2db.dispName = "Ocupación actual"
      Ocupacion : Submodelo_ANT_V1_0.ANT_OcupacionTipo;
      /** Existe dependencia económica del familiar del aspirante o solicitante
       */
      !!@ ili2db.dispName = "¿Depende económicamente?"
      Depende_Economicamente : BOOLEAN;
    END ANT_PersonaCaracterizacion;

    /** Datos de contacto de la persona.
     */
    !!@ ili2db.dispName = "(ANT) Persona contacto"
    CLASS ANT_PersonaContacto
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.ObjetoVersionado =
      /** Numero de telefono 1 de contacto de la persona.
       */
      !!@ ili2db.dispName = "Teléfono 1"
      Telefono1 : MANDATORY TEXT*20;
      /** Numero de telefono 2 de contacto de la persona.
       */
      !!@ ili2db.dispName = "Teléfono 2"
      Telefono2 : TEXT*20;
      /** Correo electrónico de la persona.
       */
      !!@ ili2db.dispName = "Correo electrónico"
      Correo_Electronico : TEXT*100;
      /** Dirección de donde vive la persona.
       */
      !!@ ili2db.dispName = "Dirección"
      Direccion : TEXT*255;
    END ANT_PersonaContacto;

    /** Datos de adicionales de la ubicación del predio.
     */
    !!@ ili2db.dispName = "(ANT) Ubicación del predio"
    CLASS ANT_UbicacionPredio
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.ObjetoVersionado =
      /** Nombre del corregimiento según el instrumento de planeación (POT, PBOT, EOT) donde se encuentra ubicado el predio objeto de intervención.
       */
      !!@ ili2db.dispName = "Corregimiento"
      Corregimiento : TEXT*255;
      /** Nombre de la vereda según el instrumento de planeación (POT, PBOT, EOT) donde se encuentra ubicado el predio objeto de intervención. En caso de no contar nombre en el instrumento de planeación tomar los nombres de acuerdo a la clasificación del DANE.
       */
      !!@ ili2db.dispName = "Vereda"
      Vereda : TEXT*255;
      /** Nombre de la vereda donde se encuentra ubicado el predio objeto de intervención según la comunidad.
       */
      !!@ ili2db.dispName = "Vereda comunitaria"
      Vereda_Comunitaria : TEXT*255;
      /** Nombre del centro poblado donde se encuentra ubicado el predio objeto de intervención.
       */
      !!@ ili2db.dispName = "Centro poblado"
      Centro_Poblado : TEXT*255;
      /** Nombre de la Unidad de Intervención según el POSPR. En caso de haber acordado con la SPO otra UI colocar ese nombre.
       */
      !!@ ili2db.dispName = "Unidad de intervención"
      Unidad_Intervencion : TEXT*255;
    END ANT_UbicacionPredio;

    ASSOCIATION ant_contactoInteresado =
      ant_persona -- {0..*} ANT_Persona;
      lc_interesado (EXTERNAL) -- {0..1} Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral.LC_Interesado;
    END ant_contactoInteresado;

    ASSOCIATION ant_otrapersonacontacto =
      ant_personaprincipal -- {0..1} ANT_Persona;
      ant_persona -- {0..*} ANT_Persona;
    END ant_otrapersonacontacto;

    ASSOCIATION ant_persona_ant_personacaracterizacion =
      ant_persona -- {1} ANT_Persona;
      ant_personacaracterizacion -- {0..1} ANT_PersonaCaracterizacion;
    END ant_persona_ant_personacaracterizacion;

    ASSOCIATION ant_persona_ant_personacontacto =
      ant_persona -- {1} ANT_Persona;
      ant_personacontacto -- {0..1} ANT_PersonaContacto;
    END ant_persona_ant_personacontacto;

    ASSOCIATION lc_predio_ant_ubicacionpredio =
      lc_predio (EXTERNAL) -- {1} Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral.LC_Predio;
      ant_ubicacionpredio -- {0..1} ANT_UbicacionPredio;
    END lc_predio_ant_ubicacionpredio;

  END ANT;

END Submodelo_ANT_V1_0.

MODEL Submodelo_DTJ_V1_0 (es)
AT "mailto:JONNY@localhost"
VERSION "2020-10-16"  =
  IMPORTS LADM_COL_V3_0,Submodelo_ANT_V1_0;

  DOMAIN

    DTJ_CambiosConcentracionPredialTipo = (
      /** Se debe marcar esta opción cuando se evidencie que el centro poblado sea diferente al que esté determinado en el plan de ordenamiento territorial.
       */
      !!@ ili2db.dispName = "Transformación del centro poblado"
      Transformacion_Centro_Poblado,
      /** Se debe marcar cuando se encuentra un nuevo centro poblado en territorio que no esté determinado ni delimitado en plan de ordenamiento territorial.
       */
      !!@ ili2db.dispName = "Nuevo nucleo o concentracion predial"
      Nuevo_Nucleo_O_Concentracion_Predial,
      /** Se marca esta opción cuando no aplica ninguna de las anteriores.
       */
      !!@ ili2db.dispName = "No aplica"
      No_Aplica
    );

    DTJ_CategoriaSueloRuralTipo = (
      /** Se debe marcar esta opción cuando el suelo rural su uso esté destinado para el desarrollo de actividades agropecuarias, forestales y mineras. Así mismo, se marcara esta opción cuando no se encuentre en el plan de ordenamiento territorial las categorías de protección o desarrollo restringido de manera específica.
       */
      !!@ ili2db.dispName = "Rural"
      Rural,
      /** Esta categoría puede existir en cualquiera de los tipos de suelo (urbano, expansión urbana y rural). Son áreas que se protegen en razón a las características geográficas, paisajísticas o ambientales. En esta subclase de suelo puede encontrarse:
       * 
       * 1. Conservación y protección del medio ambiente y recursos naturales
       * 2. Producción agrícola, ganadera, explotación recursos naturales (clases agrológicas I,II,III)
       * 3. Patrimonio Cultural
       * 4. Sistema Servicios Públicos domiciliarios.
       * 5. Amenazas y Riesgos
       */
      !!@ ili2db.dispName = "Protección"
      Proteccion,
      /** Son áreas que  no se encuartan demarcadas como suelo de protección y permiten el desarrollo de núcleos de población rural para el desarrollo de actividades económicas o dotación de equipamientos. Esta opción se deberá seleccionar cuando en el Plan de Ordenamiento Territorial Correspondientes se encuentre las siguientes categorías: 
       * 1. Suburbano
       * 2. Áreas de Vivienda Campestre
       * 3. Centros poblados 
       * 4. Equipamientos
       */
      !!@ ili2db.dispName = "Desarrollo restringido"
      Desarrollo_Restringido
    );

    DTJ_CuentaPlanParcialTipo = (
      /** Existe un plan parcial aprobado por el alcalde municipal o distrital a través de un decreto, en los términos previstos en el artículo 27 de la Ley 388 de 1997.
       */
      !!@ ili2db.dispName = "Si"
      Si,
      /** No existe aprobado ni en trámite un plan parcial.
       */
      !!@ ili2db.dispName = "No"
      No,
      /** Se encuentra en curso un plan parcial.
       */
      !!@ ili2db.dispName = "En proceso de aprobación"
      En_Proceso_Aprobacion
    );

    DTJ_DesarrolloRestringidoTipo = (
      /** Son áreas que se encuentran ubicadas dentro del suelo rural, e las cuales puede haber mezcla de usos del suelo y las formas de vida del campo y la ciudad. (Art. 34 L. 388/97).
       */
      !!@ ili2db.dispName = "Suburbano"
      Suburbano,
      /** Áreas destinadas para vivienda campestre cuyo desarrollo estará definido por las normas urbanísticas de parcelación, de acuerdo con lo previsto en Decreto 3600 de 2007, compilado en el Decreto 1077 de 2015.
       */
      !!@ ili2db.dispName = "Área de vivienda campestre"
      Area_Vivienda_Campestre,
      /** Cuando en el componente del suelo rural de los POT se incluya la delimitación de los centros poblados rurales se deberá elegir esta variable. Consultar los artículos 2.2.2.2.3.1 y 2.2.2.2.3.2 del Decreto 1077 de 2015.
       */
      !!@ ili2db.dispName = "Centro poblado rural"
      Centro_Poblado_Rural,
      /** Se debe marcar esta opción cuando se encuentre edificaciones o instalaciones que pueden ser de uso público o privado, destinadas al uso de los ciudadanos, servicios colectivos como educación, cultural, salud, recreación, deporte, religioso entre otros.
       */
      !!@ ili2db.dispName = "Equipamientos"
      Equipamientos
    );

    DTJ_EstadoSolicitudRTDAFTipo = (
      /** Marcar en caso de encontrar una solicitud de RTDAF con el predio estudiado.
       */
      !!@ ili2db.dispName = "Solicitud"
      Solicitud,
      /** Marca si se evidencia que la solicitud está siendo valorada por la URT.
       */
      !!@ ili2db.dispName = "Inicio de estudio"
      Inicio_Estudio,
      /** Marcar si se evidencia que el predio objeto de estudio está inscrito en el RTDAF.
       */
      !!@ ili2db.dispName = "Inscripción"
      Inscripcion,
      /** Marcar si en el predio si sobre el predio si se evidencia la existencia de una demanda de restitución de tierras.
       */
      !!@ ili2db.dispName = "Demanda"
      Demanda,
      /** Marcar esta opción si se evidencia la existencia de una sentencia, proferida en el marco del proceso de restitución de tierras.
       */
      !!@ ili2db.dispName = "Sentencia"
      Sentencia
    );

    DTJ_FormalidadTipo = (
      /** De acuerdo con lo expresado por la Resolución 388 de 2020 se entiende por formalidad como el "derecho real de dominio completo; es decir, que goza de un título debidamente concebido y registrado ante la Oficina de Registro de Instrumentos Públicos."
       */
      !!@ ili2db.dispName = "Formal"
      Formal,
      /** De acuerdo con lo expresado por la Resolución 388 de 2020 se entiende por informalidad el "derecho de dominio incompletos en razón a que pueden tener o no título y estar o no registrado ante la Oficina de Registro de Instrumentos Públicos."
       */
      !!@ ili2db.dispName = "Informal"
      Informal
    );

    DTJ_HallazgosErroresRegistralesTipo = (
      /** Cuando el predio en estudio aún tiene vigente su vida jurídica.
       */
      !!@ ili2db.dispName = "FMI debería estar abierto"
      FMI_Deberia_Estar_Abierto,
      /** Cuando del predio en estudio se engloben varios predios o la venta de la parte restante de ellos o se cancelen por orden judicial o administrativa los títulos o documentos que la sustentan jurídicamente y no existan anotaciones vigentes.
       */
      !!@ ili2db.dispName = "FMI debería estar cerrado"
      FMI_Deberia_Estar_Cerrado,
      /** Cuando no exista relación entre el predio segregado y el que se enuncia.
       */
      !!@ ili2db.dispName = "FMI no es segregado del que se enuncia"
