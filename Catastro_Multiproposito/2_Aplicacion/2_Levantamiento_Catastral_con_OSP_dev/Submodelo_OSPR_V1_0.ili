  TOPIC FADC =
    DEPENDS ON Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral;

    /** Define las acciones diferenciadas de una persona
     */
    !!@ ili2db.dispName = "(FADC) Acción diferenciada"
    CLASS FADC_AccionDiferenciada =
      /** Corresponde a las categorías y subcategorías del enfoque diferencial encontradas en un predio en el que se requiere el  desarrollo de acciones diferenciadas (acompañamientos) durante la visita predial. Pueden seleccionarse varias para un mismo predio.
       */
      !!@ ili2db.dispName = "Tipo de enfoque diferencial"
      Tipo_Enfoque_Diferencial : BAG {1..*} OF Submodelo_FADC_V1_0.FADC_EnfoqueDiferencialTipo_;
      /** Refiere a los elementos y detalles de la situación(es) encontrada(s) en el predio que requiere(n) acompañamiento durante la visita predial. Si se identifican varias situaciones hay que dar cuenta de ello. Esta es la base para el desarrollo de la acción diferenciada.
       */
      !!@ ili2db.dispName = "Descripción de la situación"
      Descripcion_Situacion : MANDATORY TEXT*500;
      /** Corresponde a los elementos y detalles necesarios para comprender en qué consistió y como se llevó a cabo la acción diferenciada. Es importante que se describa cómo se realizó, el papel de los responsables y en general del desarrollo de la acción diferenciada.
       */
      !!@ ili2db.dispName = "Descripción de la acción diferenciada"
      Descripcion_Accion_Diferenciada : MANDATORY TEXT*500;
      /** Alude a la(s) persona(s) que acompañan o apoyan el desarrollo de la acción diferenciada. Si bien la responsabilidad de la acción diferenciada recae en el socio, bajo orientaciones de la SPO, siempre será necesario acudir a distintos actores del municipio para llevar a cabo dichas acciones. Estos son los actores que se relacionan en este campo.
       */
      !!@ ili2db.dispName = "Responsable de implementar la acción"
      Responsable_Implementar_Accion : BAG {1..*} OF Submodelo_FADC_V1_0.FADC_ResponsableTipo_;
      /** Corresponde a la identificación de otros actores vinculados al desarrollo de la acción diferenciada, diferentes a los registrados en el campo "Responsable(s) de implementar la acción". Allí se debe especificar qué actor es, es decir, su rol.
       */
      !!@ ili2db.dispName = "Otro responsable"
      Otro_Responsable : TEXT*50;
      /** Corresponde a la fecha en la cual se llevó a cabo la acción diferenciada.
       */
      !!@ ili2db.dispName = "Fecha de implementación"
      Fecha_Implementacion : INTERLIS.XMLDate;
      /** Corresponde a la descripción del resultado de la acción diferenciada. Es importante que se incluya cómo  dicha acción contribuyó con el logro de los objetivos de la actividad (visita al predio, jornada FISO-jurídica), así como con el acceso a la oferta institucional de la ANT.
       */
      !!@ ili2db.dispName = "Resultado de la acción diferenciada"
      Resultado_Accion_Diferenciada : TEXT*500;
      /** En este campo se registran consideraciones adicionales sobre la acción diferenciada implementada.
       */
      !!@ ili2db.dispName = "Observaciones"
      Observaciones : TEXT*500;
    END FADC_AccionDiferenciada;

    /** Define los conflictos asociados a la tenencia de la tierra
     */
    !!@ ili2db.dispName = "(FADC) Conflictos"
    CLASS FADC_Conflictos =
      /** En este campo se eligen las categorías y subcategorías de conflictos que se quieren asociar a un predio
       */
      !!@ ili2db.dispName = "Tipo de conflicto"
      Tipo_Conflicto : MANDATORY Submodelo_FADC_V1_0.FADC_ConflictosTipo;
      /** En este campo debe describirse la situación de conflicto teniendo en cuenta los aspectos que caracterizan un conflicto y que están dispuestos en el documento Guía para el manejo de conflictos: situación de los actores frente al conflicto, relación entre los actores, contexto del conflicto, nivel del conflicto, fase del conflicto. Es importante lograr descripciones que resulten útiles para las decisiones que sea necesario tomar en relación al conflicto.
       */
      !!@ ili2db.dispName = "Describa el conflicto identificado"
      Descripcion_Conflicto : MANDATORY TEXT*1000;
      /** En este campo deben marcarse si en efecto hubo gestiones previas a la fase de implementación que desarrolla la Agencia. Ofrece las opciones de marcar si o no.
       */
      !!@ ili2db.dispName = "¿Se ha realizado alguna gestión previa al BPM del conflicto o denuncia ante alguna autoridad?"
      Gestion_Previa_BMP : MANDATORY BOOLEAN;
      /** Si se marcó previamente que existieron gestiones previas en relación con el conflicto, en este campo deben registrarse teniendo en cuenta en lo posible los datos que solicita el campo. Por ejemplo si existen denuncias o actas de mediación o conciliación previas que permitan ver el proceso del conflicto.
       */
      !!@ ili2db.dispName = "Indique con qué autoridad, en qué consistió la gestión, la fecha y el resultado"
      Indique_Autoridad : TEXT*500;
      /** En este campo se debe seleccionar una de las cuatro opciones que están a criterio del socio y en relación con las caracteristicas del conflicto. Es de anotar que se vincula a este campo la opción "No se realiza gestión". Si esta última opción se marca, deberá describirse las razones en el siguiente campo.
       */
      !!@ ili2db.dispName = "Tipo de gestión"
      Tipo_Gestion : MANDATORY Submodelo_FADC_V1_0.FADC_GestionTipo;
      /** En este campo deben registrarse las razones por las cuales no se realiza gestión frente a la situación de conflicto.
       */
      !!@ ili2db.dispName = "¿Motivo por el cual no realizó la gestión?"
      Motivo_No_Realizo_Gestion : TEXT*250;
      /** Alude a la(s) persona(s) que acompañan o apoyan la gestión del conflicto. Siempre será necesario acudir a distintos actores del municipio para llevar a cabo dichas acciones. Estos son los actores que se relacionan en este campo.
       */
      !!@ ili2db.dispName = "Señale quién realizará la mediación o conciliación"
      Quien_Realizara_Mediacion : Submodelo_FADC_V1_0.FADC_MediadorTipo;
      /** En este campo debe especificarse quién acompañó la gestión del conflicto cuando previamente se marco la opción otro.
       */
      !!@ ili2db.dispName = "Si su respuesta fue otro, mencione ¿cuál?"
      Otro_Realizara_Mediacion : TEXT*255;
      /** Corresponde a los elementos y detalles necesarios para comprender en qué consistió y como se llevó a cabo la acción diferenciada. Es importante que se describa cómo se realizó, el papel de los responsables y en general del desarrollo de la acción diferenciada.
       */
      !!@ ili2db.dispName = "Descripción de la estrategia de gestión del conflicto"
      Estrategia_Gestion_Conflicto : TEXT*500;
      /** Debe indicarse si el conflicto impidio realizar el levantamiento planimetrico
       */
      !!@ ili2db.dispName = "¿El conflicto impidió hacer el levantamiento planimétrico predial o el diligenciamiento del FLCM ?"
      Conflicto_Impidio_LPP_FLCM : MANDATORY BOOLEAN;
      /** Corresponde a la fecha en la cual se llevaron a cabo las acciones para atender un conflicto.
       */
      !!@ ili2db.dispName = "Fecha en la que se realizó la gestión"
      Fecha_Gestion : INTERLIS.XMLDate;
      /** Corresponde a los resultados de las acciones implementadas para manejar un conflicto. Es fundamental señalar si se pudo cumplir con el objetivo de la actividad.
       */
      !!@ ili2db.dispName = "Resultado de la gestión del conflicto"
      Resultado_Gestion : TEXT*1000;
      /** En este campo se registran consideraciones adicionales sobre el conflicto.
       */
      !!@ ili2db.dispName = "Observación"
      Observacion : TEXT*1000;
    END FADC_Conflictos;

    /** Define los atributos iniciales del formulario de acciones diferenciadas y conflictos.
     */
    !!@ ili2db.dispName = "(FADC) Datos iniciales"
    CLASS FADC_DatosIniciales =
      /** Alude a la Unidad de Intervención en el cual se ubica el predio al que está asociada la acción diferenciada, el conflicto o la situación particular.  La Unidad de Intervención es una unidad geográfica de análisis y actuación territorial que ha definido la ANT para organizar su proceso de intervención en el marco del barrido predial al interior de cada uno de los municipios. Su aplicación busca la agrupación de veredas o corregimientos que permitan priorizar la actuación en campo, y se hace mediante la valoración técnica del comportamiento de variables físicas, sociales, jurídicas y de condiciones de seguridad, al mismo tiempo que la concertación comunitaria. (ANT).
       */
      !!@ ili2db.dispName = "Unidad de intervención"
      Unidad_Intervencion : MANDATORY TEXT*100;
      /** Alude a la Vereda según en el cual se ubica el predio al que está asociada la acción diferenciada, el conflicto o la situación particular. En este caso la vereda corresponde a las reportadas por el DANE. De acuerdo con el DANE (2018) la vereda es una división territorial de carácter administrativo en el área rural de los municipios, establecida mediante acuerdo municipal. Se concibe como una agrupación comunitaria de base territorial y principal espacio de sociabilidad, caracterizada por la proximidad de residencia de sus miembros, el sentido de pertenencia e identidad común y el predominio de las relaciones vecinales. Se conforma principalmente por la agrupación de predios delimitados por accidentes geográficos y vías principales. Para el caso de los productos cartográficos censales rurales, la vereda se identifica con un texto dentro de las respectivas áreas operativas y facilita la ubicación del grupo operativo en campo.
       */
      !!@ ili2db.dispName = "Vereda"
      Vereda : TEXT*255;
      /** Alude a las veredas reportadas por la comunidad durante la Fase de Formulación de los POSPR. Una vereda comunitaria corresponde a la división geopolítica que las personas que habitan en los municipios definen para ordenar su territorio. Esta organización territorial  está basada en aspectos físicos, sociales, económicos y ambientales de las zonas y no siempre coinicide con las veredas de fuentes como el POT, el SISBEN, el DANE, etc.
       */
      !!@ ili2db.dispName = "Vereda comunitaria"
      Vereda_Comunitaria : TEXT*255;
      /** Fecha en la que se realiza el registro en el FADC.
       */
      !!@ ili2db.dispName = "Fecha de diligenciamiento del formulario"
      Fecha_Diligenciamiento_Formulario : MANDATORY INTERLIS.XMLDate;
      /** Corresponde al componente de la etapa de operativo en campo y análisis (Fase de Implementación) en el cual se realiza el registro de la acción diferenciada, el conflicto o la situación particular. Es importante señalar que este campo no contempla todos los componentes de dicha etapa, sino aquellos donde es fundamental la realización de los registros porque hay interacción con las personas que habitan los municipios intervenidos.
       */
      !!@ ili2db.dispName = "Momento de la ruta en el cual se identifica la necesidad"
      Momento_Ruta : MANDATORY Submodelo_FADC_V1_0.FADC_MomentoRutaTipo;
    END FADC_DatosIniciales;

    /** Persona de contacto alterno de la persona interesada o que tiene relación de tenencia con el predio.
     */
    !!@ ili2db.dispName = "(FADC) Otra persona de contacto"
    CLASS FADC_OtraPersonaContacto =
      /** Primer nombre de la persona que se registra como otro dato de contacto.
       */
      !!@ ili2db.dispName = "Primer nombre"
      Primer_Nombre : MANDATORY TEXT*100;
      /** Demas nombres de la persona que se registra como otro dato de contacto.
       */
      !!@ ili2db.dispName = "Demás nombres"
      Segundo_Nombre : TEXT*100;
      /** Primer apellido de la persona que se registra como otro dato de contacto.
       */
      !!@ ili2db.dispName = "Primer apellido"
      Primer_Apellido : MANDATORY TEXT*100;
      /** Segundo apellido de la persona que se registra como otro dato de contacto.
       */
      !!@ ili2db.dispName = "Segundo apellido"
      Segundo_Apellido : TEXT*100;
      /** Numero de teléfono celular de la persona que se registra como otro dato de contacto.
       */
      !!@ ili2db.dispName = "Teléfono 1"
      Telefono1 : MANDATORY TEXT*20;
      /** Numero de teléfono fijo de la persona que se registra como otro dato de contacto. (Si cuenta con este dato)
       */
      !!@ ili2db.dispName = "Teléfono 2"
      Telefono2 : TEXT*20;
      /** Indique el parentesco o relación que tiene el otro dato de contacto con la persona que se registró como una de las partes del conflicto.
       */
      !!@ ili2db.dispName = "Parentesco"
      Parentesco : Submodelo_FISO_V1_0.FISO_ParentescoTipo;
    END FADC_OtraPersonaContacto;

    /** Persona activa con la cual existe un conflicto en la tenencia de la tierra, este sujeto no tiene relación de tenencia con el predio.
     */
    !!@ ili2db.dispName = "(FADC) Persona externa conflicto"
    CLASS FADC_PersonaExternaConflicto =
      /** Corresponde al tipo de documento de identidad que presenta la persona registrada.
       */
      !!@ ili2db.dispName = "Tipo de documento"
      Tipo_Documento : MANDATORY Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.LC_InteresadoDocumentoTipo;
      /** Corresponde al numero de cédula de ciudadania de la persona registrada.
       */
      !!@ ili2db.dispName = "Documento de identidad"
      Documento_Identidad : TEXT*50;
      /** Primer nombre de la persona involucrada en el conflicto (siempre deben vincularse al menos dos personas cuando hay conflicto).
       */
      !!@ ili2db.dispName = "Primer nombre"
      Primer_Nombre : MANDATORY TEXT*100;
      /** Demas nombres de la persona involucrada en el conflicto.
       */
      !!@ ili2db.dispName = "Demás nombres"
      Segundo_Nombre : TEXT*100;
      /** Primer apellido de la persona involucrada en el conflicto.
       */
      !!@ ili2db.dispName = "Primer apellido"
      Primer_Apellido : MANDATORY TEXT*100;
      /** Segundo apellido de la persona involucrada en el conflicto.
       */
      !!@ ili2db.dispName = "Segundo apellido"
      Segundo_Apellido : TEXT*100;
      /** Corresponde al número de telefono celular de la persona registrada.
       */
      !!@ ili2db.dispName = "Teléfono 1"
      Telefono1 : TEXT*20;
      /** Corresponde al número de telefono fijo de la persona registrada.
       */
      !!@ ili2db.dispName = "Teléfono 2"
      Telefono2 : TEXT*20;
    END FADC_PersonaExternaConflicto;

    /** Define las situaciones particulares que imposibilitan el acceso al predio.
     */
    !!@ ili2db.dispName = "(FADC) Situación particular"
    CLASS FADC_SituacionParticular =
      /** En este campo se eligen el tipo de situaciones particulares que se encontraron para un predio. Pueden seleccionarse varias para un mismo predio.
       */
      !!@ ili2db.dispName = "Tipo de situación particular"
      Situacion_Particular : BAG {1..*} OF Submodelo_FADC_V1_0.FADC_SituacionParticularTipo_;
      /** Esta opción se marca cuando hay sitauciones particulares distintas a las opciones que establece el campo "Tipo de situación".
       */
      !!@ ili2db.dispName = "¿Cuál?"
      Otra_Situacion : TEXT*50;
    END FADC_SituacionParticular;

    ASSOCIATION fadc_datosiniciales_fadc_conflictos =
      fadc_datosiniciales -- {1} FADC_DatosIniciales;
      fadc_conflictos -- {1..*} FADC_Conflictos;
    END fadc_datosiniciales_fadc_conflictos;

    ASSOCIATION fadc_datosiniciales_fadc_situacionparticular =
      fadc_datosiniciales -- {1} FADC_DatosIniciales;
      fadc_situacionparticular -- {0..1} FADC_SituacionParticular;
    END fadc_datosiniciales_fadc_situacionparticular;

    ASSOCIATION lc_interesado_fadc_acciondiferenciada =
      lc_interesado (EXTERNAL) -- {1} Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral.LC_Interesado;
      fadc_acciondiferenciada -- {0..1} FADC_AccionDiferenciada;
    END lc_interesado_fadc_acciondiferenciada;

    ASSOCIATION lc_interesado_fadc_conflictos =
      lc_interesado (EXTERNAL) -- {1..*} Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral.LC_Interesado;
      fadc_conflictos -- {1..*} FADC_Conflictos;
    END lc_interesado_fadc_conflictos;

    ASSOCIATION lc_interesado_fadc_otrapersonacontacto =
      lc_interesado (EXTERNAL) -- {1..*} Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral.LC_Interesado;
      fadc_otrapersonacontacto -- {0..*} FADC_OtraPersonaContacto;
    END lc_interesado_fadc_otrapersonacontacto;

    ASSOCIATION lc_interesado_fadc_personaexternaconflicto =
      lc_interesado (EXTERNAL) -- {1..*} Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral.LC_Interesado;
      fadc_personaexternaconflicto -- {0..*} FADC_PersonaExternaConflicto;
    END lc_interesado_fadc_personaexternaconflicto;

    ASSOCIATION lc_predio_fadc_datosiniciales =
      lc_predio (EXTERNAL) -- {1} Modelo_Aplicacion_LADMCOL_Lev_Cat_V1_0.Levantamiento_Catastral.LC_Predio;
      fadc_datosiniciales -- {0..1} FADC_DatosIniciales;
    END lc_predio_fadc_datosiniciales;

  END FADC;

END Submodelo_FADC_V1_0.
