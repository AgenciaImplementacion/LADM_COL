       */
      !!@ ili2db.dispName = "Otro tipo de ocupación"
      Otro_Tipo_Ocupacion : TEXT*100;
    END FISO_EstructuraOcupacion;

    /** Define los atributos de la identificación de la solicitud en FISO
     */
    !!@ ili2db.dispName = "(FISO) Identificación solicitud"
    CLASS FISO_IdentificacionSolicitud
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.ObjetoVersionado =
      /** Fecha en que se realiza la inscripción.
       */
      !!@ ili2db.dispName = "Fecha de diligenciamiento de la solicitud"
      Fecha_Diligenciamiento_Solicitud : MANDATORY INTERLIS.XMLDate;
      /** Departamento donde se realiza la inscripción.
       */
      !!@ ili2db.dispName = "Departamento de la solicitud"
      Departamento_Solicitud : MANDATORY TEXT*255;
      /** Municipio donde se realiza la inscripción.
       */
      !!@ ili2db.dispName = "Municipio de la solicitud"
      Municipio_Solicitud : MANDATORY TEXT*255;
      /** Número de identificación del FISO para tramites internos en la ANT.
       */
      !!@ ili2db.dispName = "Número formulario FISO"
      Numero_Formulario_FISO : MANDATORY TEXT*7;
