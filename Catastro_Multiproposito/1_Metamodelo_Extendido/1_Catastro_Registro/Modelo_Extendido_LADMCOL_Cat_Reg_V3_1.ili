CONTRACTED MODEL Modelo_Extendido_LADMCOL_Cat_Reg_V3_1 (es)
AT "http://www.proadmintierra.info/"
VERSION "V2.2.1"  // 2018-02-19 // =
  IMPORTS ISO19107_PLANAS_V3_0,LADM_COL_V3_1;

  DOMAIN

    /** Valores para indicar el nivel de acuerdo
     */
    CR_AcuerdoTipo = (
      /** Existe un acuerdo sobre la posición del punto
       */
      !!@ ili2db.dispName = "Acuerdo"
      Acuerdo,
      /** Existe un desacuerdo sobre la posición del punto
       */
      !!@ ili2db.dispName = "Desacuerdo"
      Desacuerdo
    );

    CR_AnexoTipo = (
      /** Consiste en un sitio cubierto elaborado con el fin de resguardar de la intemperie. El nombre de enramadas, cobertizos o caney se utiliza según la forma de la cubierta y la región donde se ubique.
       */
      !!@ ili2db.dispName = "Enramadas o cobertizos o caneyes  - código 02"
      Enramadas_Cobertizos_Caneyes(
        /** Locales de venta de vehículos: Estructura concreto, cubierta cercha metálica y teja canaleta; fachada vidrio y polarizado, piso baldosín conservación general buena.
         */
        !!@ ili2db.dispName = "(Enramadas o cobertizos o caneyes) Tipo 90"
        Tipo_90,
        /** Estructura metálica; cubierta de zinc, eternit o teja de barro, pisos de cemento, conservación general buena.
         */
        !!@ ili2db.dispName = "(Enramadas o cobertizos o caneyes) Tipo 80"
        Tipo_80,
        /** Estructura en madera fina; cubierta de zinc, eternit o teja de barro pisos de cemento; conservación general buena a regular.
         */
        !!@ ili2db.dispName = "(Enramadas o cobertizos o caneyes) Tipo 60"
        Tipo_60,
        /** Estructura en madera común; cubierta de zinc, eternit o teja de barro; pisos en tierra; conservación regular a mala.
         */
        !!@ ili2db.dispName = "(Enramadas o cobertizos o caneyes) Tipo 40"
        Tipo_40
      ),
      /** Unidad construida con destino a la cría y/o protección de aves principalmente para explotación avícola. Conformada por estructura cubierta pisos y muros que son parcialmente elaborados en ladrillo y el resto descubierto o con algún material provisional (plásticos, alambres, etc.). Estos se clasifican según el estado de conservación en tipologías 80, 60, 40 y 20, donde se presentan mejores características en la 80 que en las demás.
       */
      !!@ ili2db.dispName = "Galpones o gallineros  - código 03"
      Galpones_Gallineros(
