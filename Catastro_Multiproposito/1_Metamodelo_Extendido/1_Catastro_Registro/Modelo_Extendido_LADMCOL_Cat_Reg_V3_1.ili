      Tipo : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_DerechoTipo;
      /** Indica la fracción del derecho en relación a la forma de tenencia y el área que esta ocupa dentro de la extensión del predio. ?
       * 
       * Expresado en tanto por uno (0.0000000000 a 1.0000000000).
       */
      !!@ ili2db.dispName = "Fracción de derecho"
      Fraccion_Derecho : MANDATORY TEXT*255;
    END CR_Derecho;

    /** Clase especializada de BaUnit, que describe la unidad administrativa básica para el caso de Colombia.
     * El predio es la unidad territorial legal propia de Catastro. Esta formada por el terreno y puede o no tener construcciones asociadas.
     */
    !!@ ili2db.dispName = "Predio"
    CLASS CR_Predio
    EXTENDS LADM_COL_V3_1.LADM_Nucleo.COL_UnidadAdministrativaBasica =
      /** Corresponde al codigo del departamento al cual pertenece el predio. Es asignado por DIVIPOLA y tiene 2 dígitos.
       */
      !!@ ili2db.dispName = "Departamento"
      Departamento : MANDATORY TEXT*2;
      /** Corresponde al código del municipio al cual pertenece el predio. Es asignado por DIVIPOLA y tiene 3 dígitos.
       */
      !!@ ili2db.dispName = "Municipio"
      Municipio : MANDATORY TEXT*3;
      /** Es un código único para identificar los inmuebles tanto en los sistemas de información catastral como registral. El NUPRE no implicará supresión de la numeración catastral ni registral asociada a la cédula catastral ni a la matrícula inmobiliaria actual.
       */
      !!@ ili2db.dispName = "Número único predial"
      NUPRE : MANDATORY TEXT*11;
      /** Circulo registral
       */
      !!@ ili2db.dispName = "Código ORIP"
      Codigo_ORIP : TEXT*3;
      /** Matricula inmobiliaria
       */
      !!@ ili2db.dispName = "Matrícula inmobiliaria"
      Matricula_Inmobiliaria : TEXT*80;
      /** Indica si el predio según la información registral cuenta con el valor de área en la cabida y linderos.
       */
      !!@ ili2db.dispName = "¿El predio tiene área registral?"
      Tiene_Area_Registral : MANDATORY BOOLEAN;
      /** Indica el valor del área registral del predio en metros cuadrados.
       */
      !!@ ili2db.dispName = "Área registral en metros cuadrados"
      Area_Registral_M2 : 0.00 .. 10000000000000000000000.00 [LADM_COL_V3_1.m2];
      /** se refiere a la situación actual en la que se encuentra una matricula inmobiliaria.
       */
      !!@ ili2db.dispName = "Estado del FMI"
      Estado_FMI : Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_EstadoFMITipo;
      /** Es la fecha en la cual se abrió el Folio de matricula inmobiliaria.
       */
      !!@ ili2db.dispName = "Fecha de apertura FMI"
      Fecha_Apertura_FMI : INTERLIS.XMLDate;
      /** Es la matrícula por la cual se dio apertura al predio objeto de estudio (la madre).
       */
      !!@ ili2db.dispName = "Matrícula predio matriz"
      Matricula_Matriz : BAG {0..*} OF Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.Catastro_Registro.CR_EstructuraMatriculaMatriz;
      /** Las matriculas aperturadas con base en el folio objeto de estudio (los hijos).
       */
      !!@ ili2db.dispName = "Matrícula predios segregados"
      Matricula_Segregados : BAG {0..*} OF Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.Catastro_Registro.CR_EstructuraMatriculaSegregados;
      /** Nuevo código númerico de treinta (30) dígitos, que se le asigna a cada predio y busca localizarlo inequívocamente en los documentos catastrales, según el modelo determinado por el Instituto Geográfico Agustin Codazzi.
       */
      !!@ ili2db.dispName = "Número predial"
      Numero_Predial : TEXT*30;
      /** Anterior código númerico de veinte (20) digitos, que se le asigna a cada predio y busca localizarlo inequívocamente en los documentos catastrales, según el modelo determinado por el Instituto Geográfico Agustin Codazzi.
       */
      !!@ ili2db.dispName = "Número predial anterior"
      Numero_Predial_Anterior : TEXT*20;
      !!@ ili2db.dispName = "Fecha de inscripción catastral"
      Fecha_Inscripcion_Catastral : INTERLIS.XMLDate;
      !!@ ili2db.dispName = "Año de la última mutación"
      Anio_Ultima_Mutacion : 1512 .. 2550;
      /** Dirección del predio.
       */
      !!@ ili2db.dispName = "Dirección"
      Direccion : BAG {1..*} OF LADM_COL_V3_1.LADM_Nucleo.ExtDireccion;
      /** Clasificación del suelo según el instrumento de planeación vigente.
       */
      !!@ ili2db.dispName = "Clase del suelo"
      Clase_Suelo : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_ClaseSueloTipo;
      !!@ ili2db.dispName = "Categoría del suelo"
      Categoria_Suelo : Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_CategoriaSueloTipo;
      /** Corresponde a una caracterización temática de las condiciones de predio que se está censando relacionada a aspectos físicos y jurídicos.
       */
      !!@ ili2db.dispName = "Condición del predio"
      Condicion_Predio : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_CondicionPredioTipo;
      /** Es la clasificación para fines estadísticos que se da a cada inmueble en su conjunto–terreno, construcciones o edificaciones-, en el momento de la identificación predial de conformidad con la actividad predominante que en él se desarrolle.
       */
      !!@ ili2db.dispName = "Destinación económica"
      Destinacion_Economica : Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_DestinacionEconomicaTipo;
      !!@ ili2db.dispName = "Tipo de predio"
      Tipo (EXTENDED) : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_PredioTipo;
      /** El avalúo catastral consiste en la determinación del valor de los predios, obtenido mediante investigación y análisis estadístico del mercado inmobiliario. ?
       * El avalúo catastral de cada predio se determinará por la adición de los avalúos parciales practicados independientemente para los terrenos y para las unidades de construcción en él comprendidos.?
       * El avalúo catastral corresponde a un porcentaje adoptado del avalúo comercial según el estudio económico, dichos avalúos se redondean a miles.
       */
      !!@ ili2db.dispName = "Avalúo Catastral"
      Avaluo_Catastral : BAG {0..*} OF Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.Catastro_Registro.CR_EstructuraAvaluoCatastral;
      /** Es la estimación del valor de un inmueble a una fecha determinada conforme a las reglas del mercado inmobiliario y la normatividad vigente.
       */
      !!@ ili2db.dispName = "Avalúo comercial"
      Avaluo_Comercial : LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Se considerará como procedimientos catastrales con efectos registrales los siguientes: actualización de linderos, rectificación de área por imprecisa determinación, actualización masiva y puntual de linderos y áreas, rectificación de linderos por acuerdo entre las partes, e inclusión de área y/o linderos.
       */
      !!@ ili2db.dispName = "Procedimientos catastrales con efectos registrales"
      Procedimiento_Catastral_Registral : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_ProcedimientoCatastralRegistralTipo;
      UNIQUE NUPRE; 
      !!@ name = "ValidacionValoresPredioTipo"
      MANDATORY CONSTRAINT Tipo <> #Ordenamiento_Territorial ;
      MANDATORY CONSTRAINT Tipo <> #Servicios_Publicos ;
      MANDATORY CONSTRAINT Tipo <> #Reservas_Naturales ;
      MANDATORY CONSTRAINT Tipo <> #Parques_Naturales ;
      MANDATORY CONSTRAINT Tipo <> #Amenazas_Riesgos ;
      MANDATORY CONSTRAINT Tipo <> #Servidumbre ;
      MANDATORY CONSTRAINT Tipo <> #Superficies_Agua ;
      MANDATORY CONSTRAINT Tipo <> #Transporte ;
    END CR_Predio;

    /** Puntos que definen los limites de las construcciones, unidad de construcción y servidumbres de transito.
     */
    !!@ ili2db.dispName = "Punto Levantamiento"
    CLASS CR_PuntoLevantamiento
    EXTENDS LADM_COL_V3_1.LADM_Nucleo.COL_Punto =
      /** Nombre o código que identifica el punto de levantamiento.
       */
      !!@ ili2db.dispName = "ID del punto de levantamiento"
      ID_Punto_Levantamiento : MANDATORY TEXT*255;
      /** Indica la clasificación del tipo de punto de acuerdo a sus características.
       */
      !!@ ili2db.dispName = "Tipo de punto"
      PuntoTipo (EXTENDED) : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_PuntoTipo;
      /** Indica si el tipo de punto corresponde a una construcción o servidumbre de tránsito.
       */
      !!@ ili2db.dispName = "Tipo de punto de levantamiento"
      Tipo_Punto_Levantamiento : Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_PuntoLevTipo;
      /** Es el método utilizado para definir puntos de terreno que son identificables dentro de una fotografía, a los cuales se les puede determinar las coordenadas terrestres de linderos y vértices una vez realizada su digitalización.
       */
      !!@ ili2db.dispName = "Fotoidentificación"
      Fotoidentificacion : Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_FotoidentificacionTipo;
      /** Es la precisión con referencia a la posición vertical o altura de un punto o conjunto de puntos en términos de distancia a un cierto plano o nivel de referencia expresada en metros.
       */
      !!@ ili2db.dispName = "Exactitud horizontal"
      Exactitud_Horizontal : MANDATORY 0.000 .. 10.000 [INTERLIS.m];
      /** Corresponde a la exactitud vertical del punto levantamiento
       */
      !!@ ili2db.dispName = "Exactitud vertical"
      Exactitud_Vertical : 0.000 .. 10.000 [INTERLIS.m];
      !!@ name = "ValidacionValoresPuntoTipo"
      MANDATORY CONSTRAINT PuntoTipo <> #Control ;
      MANDATORY CONSTRAINT PuntoTipo <> #Otro ;
    END CR_PuntoLevantamiento;

    /** es la obligación de hacer algo relacionado con el predio.
     */
    !!@ ili2db.dispName = "Responsabilidad"
    CLASS CR_Responsabilidad
    EXTENDS CR_DRR =
      /** los tipos se desarrollan por el origen de la responsabilidad.
       */
      !!@ ili2db.dispName = "Tipo"
      Tipo : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_ResponsabilidadTipo;
    END CR_Responsabilidad;

    /** Restricción legalmente constituida y registrada que afecta el derecho de una persona en relación con el predio.
     */
    !!@ ili2db.dispName = "Restricción"
    CLASS CR_Restriccion
    EXTENDS CR_DRR =
      /** son los tipos de restricciones de acuerdo con la naturaleza del registro colombiano.c
       */
      !!@ ili2db.dispName = "Tipo"
      Tipo : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_RestriccionTipo;
    END CR_Restriccion;

    /** Son las características de diseño y constructivas especiales de las construcciones y/o edificaciones.
     */
    !!@ ili2db.dispName = "(CR) Tipología de la construcción"
    CLASS CR_TipologiaConstruccion
    EXTENDS CR_CalificacionUnidadConstruccion =
      /** Son las características de diseño y constructivas especiales de las construcciones y/o edificaciones.
       */
      !!@ ili2db.dispName = "Tipo de tipología"
      Tipo_Tipologia : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_TipologiaTipo;
      !!@ ili2db.dispName = "¿Cuál?"
      Cual : TEXT*255;
    END CR_TipologiaConstruccion;

    ASSOCIATION cr_objetoconstruccion_grupocalificacion =
      cr_grupo_calificacion -- {1} CR_GrupoCalificacion;
      cr_objeto_construccion -- {0..*} CR_ObjetoConstruccion;
    END cr_objetoconstruccion_grupocalificacion;

    ASSOCIATION cr_publicidadBAUnit =
      cr_publicidad -- {0..*} CR_Publicidad;
      cr_baunit -- {1} LADM_COL_V3_1.LADM_Nucleo.COL_UnidadAdministrativaBasica;
    END cr_publicidadBAUnit;

    ASSOCIATION cr_publicidadFuente =
      cr_publicidad -- {0..1} CR_Publicidad;
      col_fuente -- {1..*} LADM_COL_V3_1.LADM_Nucleo.COL_FuenteAdministrativa;
    END cr_publicidadFuente;

    ASSOCIATION cr_publicidadInteresado =
      cr_publicidad -- {0..*} CR_Publicidad;
      col_interesado -- {0..1} LADM_COL_V3_1.LADM_Nucleo.COL_Interesado;
    END cr_publicidadInteresado;

    /** Es un tipo de restricción específico que persigue al bien independientemente de la parte interesada que respalde la acreencia.
     */
    !!@ ili2db.dispName = "Hipoteca"
    CLASS CR_Hipoteca
    EXTENDS CR_Restriccion =
    END CR_Hipoteca;

    /** La servidumbre predial o simple servidumbre, es un gravamen sobre un predio, en utilidad de otro predio de distinto dueño
     */
    !!@ ili2db.dispName = "Servidumbre de Paso"
    CLASS CR_ServidumbreTransito
    EXTENDS LADM_COL_V3_1.LADM_Nucleo.COL_UnidadEspacial =
      /** Área total de la servidumbre metros cuadrados.
       */
      !!@ ili2db.dispName = "Área de la servidumbre"
      Area_Servidumbre : MANDATORY 0.0 .. 99999999999999.9 [LADM_COL_V3_1.m2];
    END CR_ServidumbreTransito;

    ASSOCIATION cr_grupo_calificacionconvencional =
      cr_calificacion_convencional -- {1} CR_CalificacionConvencional;
      cr_grupo_calificacion -- {1..*} CR_GrupoCalificacion;
    END cr_grupo_calificacionconvencional;

    ASSOCIATION cr_ph_predio =
      cr_predio -- {1} CR_Predio;
      cr_datos_ph -- {0..1} CR_DatosPHCondominio;
    END cr_ph_predio;

    /** Relación entre unidades prediales y predios matrices bajo el régimen de propiedad horizontal y condominio.
     */
    ASSOCIATION cr_predio_copropiedad =
      /** Area privada de terreno matriz, adicional a la participación en el terreno común, según el coeficiente allí determinado.
       */
      unidad_predial -- {0..*} CR_Predio;
      /** Unidades residenciales tales como edificios, conjuntos y tipos de urbanizaciones cerradas en las que se comparten áreas comunes y que tienen régimen de propiedad horizontal.
       */
      matriz -<> {0..1} CR_Predio;
      /** Coeficiente de copropiedad de la unidad predial.
       */
      !!@ ili2db.dispName = "Coeficiente"
      Coeficiente : 0.0000000000 .. 1.0000000000;
    END cr_predio_copropiedad;

    /** Es cada conjunto de materiales consolidados dentro de un predio que tiene unas características específicas en cuanto a elementos constitutivos físicos y usos de la misma.
     */
    !!@ ili2db.dispName = "Unidad de Construcción"
    CLASS CR_UnidadConstruccion
    EXTENDS LADM_COL_V3_1.LADM_Nucleo.COL_UnidadEspacial =
      /** Identificado de la unidad de construcción, su codificación puede ser por letras del abecedario.
       */
      !!@ ili2db.dispName = "Identificador"
      Identificador : MANDATORY TEXT*20;
      /** Código de identificación de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Código de la unidad de construcción"
      Codigo_Unidad_Construccion : TEXT*30;
      /** Es el conjunto de elementos constructivos que conforman una edificación y que de acuerdo a sus características pueden ser consideradas convencionales cuando son de uso residencial, comercial e industrial o no convencionales cuando son anexos de la construcción.
       */
      !!@ ili2db.dispName = "Tipo de construcción"
      Tipo_Construccion : Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_ConstruccionTipo;
      /** Indica el tipo de dominio de la unidad de construcción: común y privado.
       */
      !!@ ili2db.dispName = "Tipo de dominio"
      Tipo_Dominio : Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_DominioConstruccionTipo;
      /** Es la caracterización del conjunto de valores que describe a un atributo de la unidad de construcción para darle condición de dominio común o dominio privado.
       */
      !!@ ili2db.dispName = "Tipo de unidad de construcción"
      Tipo_Unidad_Construccion : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_UnidadConstruccionTipo;
      /** Indica el tipo de planta donde se ubica la unidad de construcción.
       */
      !!@ ili2db.dispName = "Tipo de planta"
      Tipo_Planta : MANDATORY Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_ConstruccionPlantaTipo;
      /** Indica numéricamente la ubicación del predio de acuerdo al tipo de planta.
       */
      !!@ ili2db.dispName = "Planta ubicación"
      Planta_Ubicacion : MANDATORY 0 .. 500;
      /** Número total de  habitaciones en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de habitaciones"
      Total_Habitaciones : 0 .. 999999;
      /** Número total de baños en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de baños"
      Total_Banios : 0 .. 999999;
      /** Número total de locales en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de locales"
      Total_Locales : 0 .. 999999;
      /** Destinación de los materiales de una unidad constructiva con respecto a la actividad económica desarrollada en la edificación del predio.
       */
      !!@ ili2db.dispName = "Uso"
      Uso : Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.CR_UsoUConsTipo;
      /** Año de construcción de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Año de construcción"
      Anio_Construccion : 1512 .. 2500;
      /** Para el avalúo de la unidad constructiva, en caso de modelos econométricos, se deben tener en cuenta el tipo de estructura, los acabados, el baño, la cocina, el estado de conservación, la edad de la edificación, el uso que se le da a dichas construcciones, entre otros, lo cual es valorado de acuerdo con las condiciones que presente el inmueble.?
       * Para el caso de actualización de Zonas Homogéneas y tablas de construcción se utilizan variables de puntaje, uso de la construcción y área construida para la determinación del avalúo.?
       * El avalúo catastral de la unidad corresponde a un porcentaje adoptado del avalúo comercial según el estudio económico.
       */
      !!@ ili2db.dispName = "Avalúo de la unidad de construcción"
      Avaluo_Unidad_Construccion : LADM_COL_V3_1.LADM_Nucleo.Peso;
      !!@ ili2db.dispName = "Avalúo comercial unidad construcción"
      Avaluo_Comercial_Unidad_Construccion : LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Área total construida en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Área construida"
      Area_Construida : MANDATORY 0.0 .. 99999999999999.9 [LADM_COL_V3_1.m2];
      /** Área total privada de la unidad de construcción para los predios en régimen de propiedad horizontal.
       */
      !!@ ili2db.dispName = "Área privada construida"
      Area_Privada_Construida : 0.0 .. 99999999999999.9 [LADM_COL_V3_1.m2];
      /** Altura total de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Altura"
      Altura : 0.00 .. 1000.00 [INTERLIS.m];
      /** Observaciones generales respecto de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Observaciones"
      Observaciones : TEXT;
    END CR_UnidadConstruccion;

    ASSOCIATION cr_hipotecaDerecho =
      cr_hipoteca -- {0..*} CR_Hipoteca;
      cr_derecho -- {0..*} CR_Derecho;
    END cr_hipotecaDerecho;

    /** Relación entre la clase Unidad_Construccion del modelo de Avalúos y Calificacion_Unidad_Construccion de modelo de Avalúos
     */
    ASSOCIATION cr_calificacion_unidadconstruccion =
      /** Identificado de la unidad de construcción, su codificación puede ser por letras del abecedario.
       */
      cr_unidad_construccion (EXTERNAL) -- {1..*} CR_UnidadConstruccion;
      cr_calificacion_unidad_construccion -- {0..1} CR_CalificacionUnidadConstruccion;
    END cr_calificacion_unidadconstruccion;

    ASSOCIATION cr_construccion_unidadconstruccion =
      cr_unidadconstruccion -- {0..*} CR_UnidadConstruccion;
      cr_construccion -<> {1} CR_Construccion;
    END cr_construccion_unidadconstruccion;

  END Catastro_Registro;

END Modelo_Extendido_LADMCOL_Cat_Reg_V3_1.
