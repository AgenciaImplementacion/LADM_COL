        /** Edificación sin cubierta con que se identifican las pistas de los aeropuertos construidas en asfalto o concreto, incluye la zona o calle de redaje y calles de salida rápida.
         */
        !!@ ili2db.dispName = "(Anexo) Pista aeropuerto"
        Pista_Aeropuerto,
        /** Perforaciones profundas  manuales para extraer agua subterránea.  Medida en metros de profundidad.
         */
        !!@ ili2db.dispName = "(Anexo) Pozos"
        Pozos,
        /** Consiste en un sitio cubierto elaborado con el fin de resguardar de la intemperie; el nombre de enramadas, cobertizos o caney se utiliza según la forma de la cubierta y la región donde se ubique.
         */
        !!@ ili2db.dispName = "(Anexo) Ramadas - Cobertizos - Caneyes"
        Ramadas_Cobertizos_Caneyes,
        /** Construcciones elaboradas con el objeto de secar en condiciones naturales el café principalmente, aunque es común su uso en otros productos.  Consta de estructura y pisos para su calificación
         */
        !!@ ili2db.dispName = "(Anexo) Secaderos"
        Secaderos,
        /** Construcción de grandes dimensiones que sirve para almacén o depósito, se exceptúan los silos cafeteros
         */
        !!@ ili2db.dispName = "(Anexo) Silos"
        Silos,
        /** Depósitos subterráneos o de altura, construidos en zonas con deficiencias en abastecimiento para el consumo de los habitantes o para el uso agrícola.  El elemento a calificar es la estructura relacionada con la capacidad de almacenamiento medida en metros cúbicos.
         *  
         * Se excluyen de este tipo de construcción los tanques de almacenamiento domiciliario
         */
        !!@ ili2db.dispName = "(Anexo) Tanques"
        Tanques,
        /** Deslizadores construidos alrededor de piscinas, medidos en metros lineales.  Se clasifican según su altura y materiales de construcción
         */
        !!@ ili2db.dispName = "(Anexo) Toboganes"
        Toboganes,
        /** Edificio en forma de torre, en cuya cima se sitúa una sala de control, desde la que se dirige y controla el tráfico de un puerto, de un aeropuerto o de un circuito de carreras.
         */
        !!@ ili2db.dispName = "(Anexo) Torre de control"
        Torre_de_Control,
        /** Son estructuras diseñadas para disminuir la temperatura del agua y otros medios.
         */
        !!@ ili2db.dispName = "(Anexo) Torres de Enfriamiento"
        Torres_de_Enfriamiento,
        /** Se le asigna a las unidades prediales no edificadas o por construir
         */
        !!@ ili2db.dispName = "(Anexo) Unidad predial por construir"
        Unidad_Predial_por_Construir,
        /** Parte de la infraestructura ferroviaria que une dos puntos determinados del territorio
         */
        !!@ ili2db.dispName = "(Anexo) Vía Férrea"
        Via_Ferrea
      )
    );

  /** Este Topic agrupa a las clases específicas del avalúo, así como sus atributos.
   */
  TOPIC Avaluos =
    OID AS INTERLIS.UUIDOID;
    DEPENDS ON Catastro_Registro_V3_0.Catastro_Registro;

    /** Clase los valores por M2 de construcción según el uso y puntaje de calificación de construcción.
     */
    !!@ ili2db.dispName = "(Avalúos) Tabla Calificación de la Construcción"
    CLASS AV_TablaCalificacionConstruccion =
      /** Valor por metros cuadrado de construcción clasificando por uso y puntaje de las unidades de construcción.
       */
      !!@ ili2db.dispName = "Valor M2 de construcción"
      Valor_M2_Construccion : LADM_COL_V3_0.LADM_Nucleo.Peso;
      /** Es la actividad que se desarrolla en una unidad de construcción.
       */
      !!@ ili2db.dispName = "Uso"
      Uso : Avaluos_V3_0.AV_UsoUConsTipo;
      /** Puntaje de las edificaciones de acuerdo a sus características constructivas.
       */
      !!@ ili2db.dispName = "Puntuación"
      Puntuacion : TEXT*20;
    END AV_TablaCalificacionConstruccion;

    /** Clase que relaciona las tipologias de las construcciones con su respectivo valor por M2 de construcción.
     */
    !!@ ili2db.dispName = "(Avalúos) Tipología de la construcción"
    CLASS AV_TipologiaConstruccion =
      /** Se refiere a la zonificación del municipio de acuerdo con las clasificaciones de los inmuebles según su
       * actividad residencial, comercial e industrial.
       */
      !!@ ili2db.dispName = "Tipo de tipología"
      Tipo_Tipologia : Avaluos_V3_0.AV_TipologiaTipo;
      /** Valor unitario en pesos del tipo de tipología constructiva.
       */
      !!@ ili2db.dispName = "Valor unitario"
      Valor_Unitario : LADM_COL_V3_0.LADM_Nucleo.Peso;
      UNIQUE Tipo_Tipologia; 
    END AV_TipologiaConstruccion;

    /** Clase que contiene las variables que componen las Zonas Homogeneas Fisicas rurales. Las ZHF Son espacios geográficos con características similares en cuanto a vías, topografía, servicios públicos, uso actual del suelo, norma de uso del suelo, tipificación de las construcciones y/o edificaciones, áreas homogéneas de tierra, disponibilidad de aguas superficiales permanentes u otras variables que permitan diferenciar estas áreas de las adyacentes.
     */
    !!@ ili2db.dispName = "(Avalúos) Zona homogénea física rural"
    CLASS AV_ZonaHomogeneaFisicaRural =
      /** Código único del registro de la instancia de ZonaHomegeneaFisca.
       */
      !!@ ili2db.dispName = "Código"
      Codigo : MANDATORY TEXT*20;
      /** Identificador de la zona homogénea física rural.
       */
      !!@ ili2db.dispName = "Código zona física"
      Codigo_Zona_Fisica : MANDATORY TEXT*20;
      /** Espacios de la superficie terrestre que presentan características o cualidades similares en cuanto a las condiciones de clima, relieve, material litológico superficial o depósitos superficiales y de las características internas de los suelos, condiciones que expresan la capacidad productiva de las tierras; ello se indica mediante un valor numérico denominado Valor Potencial (VP).
       */
      !!@ ili2db.dispName = "Área homogénea tierra"
      Area_Homogenea_Tierra : 0.00 .. 99999999999999990000000.00;
      /** En el estudio de esta variable se analiza la existencia de fuentes de aguas superficiales permanentes y su clasificación, con el fin de zonificar de acuerdo con la posibilidad de aprovechamiento para el uso actual que se da al suelo.
       */
      !!@ ili2db.dispName = "Disponibilidad agua"
      Disponibilidad_Agua : Avaluos_V3_0.AV_DisponibilidadAguaTipo;
      /** Se refiere a la existencia y calidad de las vías, analizada a nivel de zona. A esta condición se asocia la existencia de otras obras de ingeniería, la instalación de redes de servicios públicos, etc.
       */
      !!@ ili2db.dispName = "Influencia vial"
      Influencia_Vial : Avaluos_V3_0.AV_InfluenciaVialRuralTipo;
      /** Actividad que se desarrolla en un determinado espacio geográfico en el momento de la elaboración del estudio de zonas homogéneas físicas.
       */
      !!@ ili2db.dispName = "Uso actual del suelo"
      Uso_Actual_Suelo : Avaluos_V3_0.AV_UsoSueloRuralTipo;
      /** Indicación legal sobre la actividad que se puede desarrollar en un determinado espacio geográfico con lo planificado y reglamentado por la respectiva autoridad de la unidad orgánica catastral.
       */
      !!@ ili2db.dispName = "Norma uso del suelo"
      Norma_Uso_Suelo : TEXT*250;
      /** Vigencia del estudio de zonas homogeneas fisicas rurales.
       */
      !!@ ili2db.dispName = "Vigencia"
      Vigencia : INTERLIS.XMLDate;
      /** Definición de la geometría espacial georreferenciada de la zona homogénea física, delimitada mediante un polígono que contiene sus linderos y la superficie ocupada.
       */
      !!@ ili2db.dispName = "Geometría"
      Geometria : MANDATORY ISO19107_PLANAS_V3_0.GM_MultiSurface3D;
    END AV_ZonaHomogeneaFisicaRural;

    /** Clase que contiene las variables que componen las Zonas Homogeneas Fisicas urbanas. Las ZHF Son espacios geográficos con características similares en cuanto a vías, topografía, servicios públicos, uso actual del suelo, norma de uso del suelo, tipificación de las construcciones y/o edificaciones, áreas homogéneas de tierra, disponibilidad de aguas superficiales permanentes u otras variables que permitan diferenciar estas áreas de las adyacentes.
     */
    !!@ ili2db.dispName = "(Avalúos) Zona homogénea fisíca urbana"
    CLASS AV_ZonaHomogeneaFisicaUrbana =
      /** Código único del registro de la instancia de ZonaHomegeneaFisca.
       */
      !!@ ili2db.dispName = "Código"
      Codigo : MANDATORY TEXT*20;
      /** Identificador de la zona homogénea física urbana.
       */
      !!@ ili2db.dispName = "Código zona física"
      Codigo_Zona_Fisica : MANDATORY TEXT*20;
      /** Características naturales relacionadas con el relieve que permitan o dificulten el desarrollo de las construcciones, dentro de esta variable deben considerarse factores tales como zonas anegadizas o inundables, las discontinuidades físicas tales como taludes, caños y ríos, altura por encima de la cota de servicios, áreas potenciales de derrumbe, por inclinación o presencia de drenajes superficiales.
       */
      !!@ ili2db.dispName = "Topografía"
      Topografia : Avaluos_V3_0.AV_TopografiaZonaTipo;
      /** Se refiere a la existencia y calidad de las vías, analizada a nivel de zona. A esta condición se asocia la existencia de otras obras de ingeniería, la instalación de redes de servicios públicos, etc.
       */
      !!@ ili2db.dispName = "Influencia vial"
      Influencia_Vial : Avaluos_V3_0.AV_InfluenciaVialUrbanaTipo;
      /** Se prestan con la finalidad específica de satisfacer las necesidades esenciales de las personas. Para efectos de la presente metodología se consideran servicios públicos básicos: Acueducto, Alcantarillado y Energía Eléctrica.
       */
      !!@ ili2db.dispName = "Servicio público"
      Servicio_Publico : Avaluos_V3_0.AV_ServiciosPublicosTipo;
      /** Actividad que se desarrolla en un determinado espacio geográfico en el momento de la elaboración del estudio de zonas homogéneas físicas.
       */
      !!@ ili2db.dispName = "Uso actual del suelo"
      Uso_Actual_Suelo : Avaluos_V3_0.AV_UsoSueloUrbanoTipo;
      /** Indicación legal sobre la actividad que se puede desarrollar en un determinado espacio geográfico con lo planificado y reglamentado por la respectiva autoridad de la unidad orgánica catastral.
       */
      !!@ ili2db.dispName = "Norma uso del suelo"
      Norma_Uso_Suelo : TEXT*250;
      /** Clasificación de las construcciones de acuerdo a ciertas características observables y cuantificables que reflejan las condiciones socioeconómicas (ingresos y cultura) de los habitantes de las viviendas.
       */
      !!@ ili2db.dispName = "Tipificación de la construcción"
      Tipificacion_Construccion : Avaluos_V3_0.AV_TipificacionConstruccionTipo;
      /** Vigencia del estudio de zonas homogeneas fisicas urbanas.
       */
      !!@ ili2db.dispName = "Vigencia"
      Vigencia : INTERLIS.XMLDate;
      /** Definición de la geometría espacial georreferenciada de la zona homogénea física, delimitada mediante un polígono que contiene sus linderos y la superficie ocupada.
       */
      !!@ ili2db.dispName = "Geometría"
      Geometria : MANDATORY ISO19107_PLANAS_V3_0.GM_MultiSurface3D;
    END AV_ZonaHomogeneaFisicaUrbana;

    /** Clase que contiene las variables que componen las Zonas Homogeneas Geoconómicas rurales. Las ZHG son los espacios geográficos determinados a partir de Zonas Homogéneas Físicas con valores unitarios similares en cuanto a su precio, según las condiciones del mercado inmobiliario.
     */
    !!@ ili2db.dispName = "(Avalúos) Zona homogénea geoeconómica urbana"
    CLASS AV_ZonaHomogeneaGeoeconomicaUrbana =
      /** Código único del registro de la instancia de la zona geoeconómica.
       */
      !!@ ili2db.dispName = "Código"
      Codigo : MANDATORY TEXT*20;
      /** Identificador de la zona homogénea geoeconómica urbana.
       */
      !!@ ili2db.dispName = "Código zona geoeconómica"
      Codigo_Zona_Geoeconomica : MANDATORY TEXT*20;
      /** Valor económico de la zona homogénea que representa la instancia de la clase.
       */
      !!@ ili2db.dispName = "Valor del metro cuadrado"
      Valor_Metro : MANDATORY LADM_COL_V3_0.LADM_Nucleo.Peso;
      /** Códigos de las zonas homogeneas fisicas urbanas que componen la zona homogénea geoeconómica urbana.
       */
      !!@ ili2db.dispName = "Subzona física"
      Subzona_Fisica : TEXT;
      /** Vigencia del estudio de zonas homogéneas geoeconómicas urbanas.
       */
      !!@ ili2db.dispName = "Vigencia"
      Vigencia : INTERLIS.XMLDate;
      /** Definición de la geometría espacial georreferenciada de la zona homogénea geoeconómica, delimitada mediante un polígono que contiene sus linderos y la superficie ocupada.
       */
      !!@ ili2db.dispName = "Geometría"
      Geometria : MANDATORY ISO19107_PLANAS_V3_0.GM_MultiSurface3D;
    END AV_ZonaHomogeneaGeoeconomicaUrbana;

    /** Esta clase permite gestionar las zonas con características económicas y de localización geográfica similares, definidas para caracterizar el avalúo.
     */
    !!@ ili2db.dispName = "(Avalúos) Zona homogénea geoeconómica rural"
    CLASS AV_ZonaHomogeneaGeoeconomicaRural =
      /** Código único del registro de la instancia de la zona geoeconómica.
       */
      !!@ ili2db.dispName = "Código"
      Codigo : MANDATORY TEXT*20;
      /** Identificador de la zona homogénea geoeconómica rural.
       */
      !!@ ili2db.dispName = "Código zona geoeconómica"
      Codigo_Zona_Geoeconomica : MANDATORY TEXT*20;
      /** Valor económico de la zona homogénea que representa la instancia de la clase.
       */
      !!@ ili2db.dispName = "Valor por hectárea"
      Valor_Hectarea : MANDATORY LADM_COL_V3_0.LADM_Nucleo.Peso;
      /** Códigos de las zonas homogeneas fisicas rurales que componen la zona homogénea geoeconómica rural.
       */
      !!@ ili2db.dispName = "Subzona física"
      Subzona_Fisica : TEXT;
      /** Vigencia del estudio de zonas homogéneas geoeconómicas rurales.
       */
      !!@ ili2db.dispName = "Vigencia"
      Vigencia : INTERLIS.XMLDate;
      /** Definición de la geometría espacial georreferenciada de la zona homogénea geoeconómica, delimitada mediante un polígono que contiene sus linderos y la superficie ocupada.
       */
      !!@ ili2db.dispName = "Geometría"
      Geometria : MANDATORY ISO19107_PLANAS_V3_0.GM_MultiSurface3D;
    END AV_ZonaHomogeneaGeoeconomicaRural;

  END Avaluos;

END Avaluos_V3_0.
