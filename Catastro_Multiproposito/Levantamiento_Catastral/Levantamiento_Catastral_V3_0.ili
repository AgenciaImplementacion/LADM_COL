    CLASS LC_UnidadConstruccion
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.COL_UnidadEspacial =
      /** Identificado de la unidad de construcción, su codificación puede ser por letras del abecedario.
       */
      !!@ ili2db.dispName = "Identificador"
      Identificador : MANDATORY TEXT*3;
      /** Indica el tipo de dominio de la unidad de construcción: común y privado.
       */
      !!@ ili2db.dispName = "Tipo de dominio"
      Tipo_Dominio : Levantamiento_Catastral_V3_0.LC_DominioConstruccionTipo;
      /** Indica si la construcción es de tipo convencional o no convencional.
       */
      !!@ ili2db.dispName = "Tipo de construcción"
      Tipo_Construccion : Levantamiento_Catastral_V3_0.LC_ConstruccionTipo;
      /** Indica si la unidad de construcción es residencial, comercial, industrial o anexo.
       */
      !!@ ili2db.dispName = "Tipo de unidad de construcción"
      Tipo_Unidad_Construccion : MANDATORY Levantamiento_Catastral_V3_0.LC_UnidadConstruccionTipo;
      /** Indica el tipo de planta donde se ubica la unidad de construcción.
       */
      !!@ ili2db.dispName = "Tipo de planta"
      Tipo_Planta : MANDATORY Levantamiento_Catastral_V3_0.LC_ConstruccionPlantaTipo;
      /** Indica numéricamente la ubicación del predio de acuerdo al tipo de planta.
       */
      !!@ ili2db.dispName = "Planta ubicación"
      Planta_Ubicacion : MANDATORY 0 .. 500;
      /** Número total de  habitaciones en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de habitaciones"
      Total_Habitaciones : 0 .. 999999;
      /** Número total de baños en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de baños"
      Total_Banios : 0 .. 999999;
      /** Número total de locales en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de locales"
      Total_Locales : 0 .. 999999;
      /** Número total de pisos en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de pisos"
      Total_Pisos : 0 .. 150;
      /** Actividad que se desarrolla en una unidad de construcción.
       */
      !!@ ili2db.dispName = "Uso"
      Uso : Levantamiento_Catastral_V3_0.LC_UsoUConsTipo;
      /** Año de construcción de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Año de construcción"
      Anio_Construccion : 1512 .. 2500;
      /** Avalúo catastral de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Avalúo de la unidad de construcción"
      Avaluo_Unidad_Construccion : LADM_COL_V3_0.LADM_Nucleo.Peso;
      /** Área total construida en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Área construida"
      Area_Construida : MANDATORY 0.0 .. 99999999999999.9 [LADM_COL_V3_0.m2];
      /** Área total privada de la unidad de construcción para los predios en régimen de propiedad horizontal.
       */
      !!@ ili2db.dispName = "Área privada construida"
      Area_Privada_Construida : 0.0 .. 99999999999999.9 [LADM_COL_V3_0.m2];
      /** Altura total de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Altura"
      Altura : 1 .. 1000 [INTERLIS.m];
      /** Observaciones generales respecto de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Observaciones"
      Observaciones : TEXT;
    END LC_UnidadConstruccion;

    /** Relación entre la clase Unidad_Construccion del modelo de Avalúos y Calificacion_Unidad_Construccion de modelo de Avalúos
     */
    ASSOCIATION lc_calificacion_unidadconstruccion =
      lc_unidad_construccion (EXTERNAL) -- {1} LC_UnidadConstruccion;
      lc_calificacion_unidad_construccion -- {0..1} LC_CalificacionUnidadConstruccion;
    END lc_calificacion_unidadconstruccion;

    ASSOCIATION lc_construccion_unidadconstruccion =
      lc_unidadconstruccion -- {0..*} LC_UnidadConstruccion;
      lc_construccion -<> {1} LC_Construccion;
    END lc_construccion_unidadconstruccion;

  END Levantamiento_Catastral;

END Levantamiento_Catastral_V3_0.
