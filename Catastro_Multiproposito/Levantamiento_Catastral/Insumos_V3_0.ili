  TOPIC Datos_Gestor_Catastral =

    /** Dato geografico aportado por el Gestor Catastral respecto de los barrios de una entidad territorial.
     */
    !!@ ili2db.dispName = "(GC) Barrio"
    CLASS GC_Barrio =
