       */
      !!@ ili2db.dispName = "Código del sector"
      Codigo_Sector : TEXT*9;
      /** Geometría en 2D de la vereda.
       */
      !!@ ili2db.dispName = "Geometría"
      Geometria : ISO19107_PLANAS_V3_0.GM_MultiSurface2D;
